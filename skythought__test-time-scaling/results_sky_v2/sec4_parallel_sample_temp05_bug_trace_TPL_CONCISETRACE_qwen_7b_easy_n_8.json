 Remove trailing zeros\n    while x.endswith('0'):\n        x = x[:-1]\n    \n    # Remove unnecessary decimal point if present\n    if x.endswith('.'):\n        x = x[:-1]\n    \n    # Print the result\n    print(x)\n\n# Call the function\nprocess_number()\n```", "```python\ndef remove_trailing_zeros_and_decimal_point():\n    import sys\n    import re\n    \n    # Read input from standard input\n    X = sys.stdin.read().strip()\n    \n    # Convert the string to a float and back to a string to remove unnecessary trailing zeros\n    X = float(X)\n    X = str(X)\n    \n    # Remove trailing zeros and unnecessary decimal point\n    X = re.sub(r'\\.0+$', '', X)  # Remove trailing zeros\n    X = re.sub(r'\\.$', '', X)    # Remove trailing decimal point if no digits follow\n    \n    # Print the result\n    print(X)\n\nremove_trailing_zeros_and_decimal_point()"]}
