 len(num_str)\n        swapped_numbers = set()\n        for i in range(n):\n            for j in range(i + 1, n):\n                swapped = num_str[:i] + num_str[j] + num_str[i + 1:j] + num_str[i] + num_str[j + 1:]\n                swapped_numbers.add(int(swapped))\n        return swapped_numbers\n    \n    num_dict = defaultdict(set)\n    for num in nums:\n        num_dict[num].add(num)\n        num_dict[num].update(generate_swapped_numbers(num))\n    \n    count = 0\n    for i in range(len(nums)):\n        for j in range(i + 1, len(nums)):\n            if nums[i] in num_dict[nums[j]]:\n                count += 1\n    \n    return count", "def count_almost_equal_pairs(nums):\n    n = len(nums)\n    count = 0\n    for i in range(n):\n        for j in range(i + 1, n):\n            num1 = str(nums[i])\n            num2 = str(nums[j])\n            if sorted(num1) == sorted(num2):\n                count += 1\n    return count", "def count_almost_equal_pairs(nums):\n    n = len(nums)\n    count = 0\n    \n    for i in range(n):\n        for j in range(i + 1, n):\n            num1 = str(nums[i])\n            num2 = str(nums[j])\n            \n            if num1 == num2:\n                count += 1\n                continue\n            \n            for k in range(len(num1)):\n                for l in range(k + 1, len(num1)):\n                    swapped_num1 = list(num1)\n                    swapped_num1[k], swapped_num1[l] = swapped_num1[l], swapped_num1[k]\n                    if ''.join(swapped_num1) == num2:\n                        count += 1\n                        break\n                else:\n                    continue\n                break\n    \n    return count", "def count_almost_equal_pairs(nums):\n    n = len(nums)\n    num_set = set(nums)\n    count = 0\n    \n    for i in range(n):\n        num_str = str(nums[i])\n        length = len(num_str)\n        \n        # Generate all unique swaps\n        seen = set()\n        for j in range(length):\n    