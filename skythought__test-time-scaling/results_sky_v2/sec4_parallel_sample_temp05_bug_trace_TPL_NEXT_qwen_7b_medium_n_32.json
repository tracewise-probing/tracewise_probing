        adj = defaultdict(set)\n        for u, v in graph:\n            adj[u].add(v)\n            adj[v].add(u)\n        return adj\n\n    def count_edges(adj):\n        return sum(len(adj[i]) - 1 for i in adj) // 2\n\n    def update_dp(state1, state2, cost):\n        new_state = state1 | state2\n        if dp[new_state][state1] > dp[state1][state2] + cost:\n            dp[new_state][state1] = dp[state1][state2] + cost\n\n    G_adj = get_adj_matrix(G)\n    H_adj = get_adj_matrix(H)\n\n    for mask in range(1 << N):\n        for i in range(N):\n            if mask & (1 << i):\n                for j in range(i + 1, N):\n                    if mask & (1 << j):\n                        edge_cost = costs[i][j]\n                        update_dp(mask, (1 << i) | (1 << j), edge_cost)\n\n    target_mask = (1 << N) - 1\n    result = float('inf')\n    for state in range(1 << N):\n        if dp[state][target_mask] < result:\n            result = dp[state][target_mask]\n\n    return result\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    M_H = int(data[index])\n    index += 1\n    H = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    costs = [[int(data[index + j * N + i]) for i in range(N)] for j in range(N - 1)]\n    index += N * (N - 1)\n\n    result = min_cost_to_isomorphize(G, H, costs)\n    print(result)", "```python\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\n# Read input data\nindex = 0\nN = int(data[index])\nindex += 1\nM_G = int(data[index])\nindex += 1\nedges_G = []\nfor _ in range(M_G):\n    u = int(data[index]) - 1\n    v = int(data[index + 1]) - 1\n    edges_G.append((u, v))\n    index += 2\n\nM_H = int(data[index])\nindex += 1\nedges_H = []\nfor _ in range(M_H):\n    a = int(data[index]) - 1\n    b = int(data[index + 1]) - 1\n    edges_H.append((a, b))\n    index += 2\n\ncosts = []\nfor i in range(1, N):\n    row = []\n    for j in range(i + 1, N):\n        row.append(int(data[index]))\n        index += 1\n    costs.append(row)\n\n# Function to check if two graphs are isomorphic\ndef is_isomorphic(graph1, graph2):\n    n = len(graph1)\n    for perm in permutations(range(n)):\n        if all(graph1[i][j] == graph2[perm[i]][perm[j]] for i in range(n) for j in range(i + 1, n)):\n            return True\n    return False\n\n# Dynamic programming to find minimum cost\nfrom collections import defaultdict\nfrom itertools import permutations\n\ndp = defaultdict(lambda: float('inf'))\ndp[0] = 0\n\nfor mask in range(1 << N):\n    count_edges = bin(mask).count('1')\n    if count_edges != len(edges_H):\n        continue\n    \n    for i in range(N):\n        for j in range(i + 1, N):\n            if (i, j) not in edges_H and (i, j) in edges_G:\n                dp[mask | (1 << i) | (1 << j)] = min(dp[mask | (1 << i) | (1 << j)], dp[mask] + costs[i][j])\n            elif (i, j) in edges_H and (i, j) not in edges_G:\n                dp[mask ^ (1 << i) ^ (1 << j)] = min(dp[mask ^ (1 << i) ^ (1 << j)], dp[mask] + costs[i][j])\n\nprint(min(dp.values()))\n```", "```python\nimport sys\nfrom itertools import permutations\nimport numpy as np\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G = np.zeros((N, N), dtype=int)\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        G[u][v] = G[v][u] = 1\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    H = np.zeros((N, N), dtype=int)\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        H[a][b] = H[b][a] = 1\n        index += 2\n    \n    A = []\n    for i in range(1, N):\n        row = list(map(int, data[index:index + N - i]))\n        A.append(row)\n        index += N - i\n    \n    return N, G, H, A\n\ndef min_cost_to_isomorphic(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        current_cost = 0\n        H_perm = H.copy()\n        \n        for i in range(N):\n            for j in range(i + 1, N):\n                if G[i][j] != H_perm[perm[i]][perm[j]]:\n                    current_cost += A[i][j]\n                    H_perm[perm[i]][perm[j]] = H_perm[perm[j]][perm[i]] = 1 - H_perm[perm[i]][perm[j]]\n        \n        min_cost = min(min_cost, current_cost)\n    \n    return min_cost\n\ndef main():\n    N, G, H, A = read_input()\n    result = min_cost_to_isomorphic(G, H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    \n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    \n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(1, N):\n        row = []\n        for j in range(i + 1, N):\n            cost = int(data[index])\n            index += 1\n            row.append(cost)\n        A.append(row)\n    \n    # Adjacency lists for G and H\n    adj_G = [[] for _ in range(N)]\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n    \n    adj_H = [[] for _ in range(N)]\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n    \n    # DP table\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    for mask in range(1 << N):\n        for i in range(N):\n            if mask & (1 << i):\n                for j in range(i + 1, N):\n                    if mask & (1 << j):\n                        if (i, j) in adj_G[mask ^ (1 << i)] != (j, i) in adj_G[mask ^ (1 << j)]:\n                            cost = A[i][j]\n                            new_mask = mask ^ (1 << i) ^ (1 << j)\n                            dp[new_mask] = min(dp[new_mask], dp[mask] + cost)\n    \n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()", "import sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorph(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n\n    # Generate all permutations of vertex mappings from G to H\n    for perm in permutations(range(1, N + 1)):\n        current_cost = 0\n        # Check each edge in G and calculate the cost to match it in H\n        for i in range(N - 1):\n            for j in range(i + 1, N):\n                if G[i][j] != H[perm[i] - 1][perm[j] - 1]:\n                    current_cost += A[i][j]\n        # Update the minimum cost if the current cost is lower\n        min_cost = min(min_cost, current_cost)\n\n    return min_cost\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G = [[False] * N for _ in range(N)]\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        index += 1\n        v = int(data[index]) - 1\n        index += 1\n        G[u][v] = True\n        G[v][u] = True\n\n    M_H = int(data[index])\n    index += 1\n    H = [[False] * N for _ in range(N)]\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        index += 1\n        b = int(data[index]) - 1\n        index += 1\n        H[a][b] = True\n        H[b][a] = True\n\n    A = []\n    for i in range(N - 1):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n\n    result = min_cost_to_isomorph(G, H, A)\n    print(result)", "```python\nimport sys\nfrom itertools import permutations\n\ndef is_isomorphic(G, H):\n    return G == H\n\ndef min_cost_to_isomorphic(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        H_perm = [[0] * N for _ in range(N)]\n        for i in range(N):\n            for j in range(i + 1, N):\n                if H[i][j] == 1:\n                    H_perm[perm[i]][perm[j]] = 1\n                else:\n                    H_perm[perm[i]][perm[j]] = 0\n        \n        if is_isomorphic(G, H_perm):\n            cost = 0\n            for i in range(N):\n                for j in range(i + 1, N):\n                    if G[i][j] != H_perm[i][j]:\n                        cost += A[i][j]\n            min_cost = min(min_cost, cost)\n    \n    return min_cost\n\ndef main():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G = [[0] * N for _ in range(N)]\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        index += 2\n        G[u][v] = 1\n        G[v][u] = 1\n    \n    M_H = int(data[index])\n    index += 1\n    H = [[0] * N for _ in range(N)]\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        index += 2\n        H[a][b] = 1\n        H[b][a] = 1\n    \n    A = []\n    for i in range(N - 1):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n    \n    result = min_cost_to_isomorphic(G, H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()", "def min_cost_to_isomorphize():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n\n    A = []\n    for i in range(N - 1):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n\n    def get_adj_matrix(edges, N):\n        adj = [[0] * N for _ in range(N)]\n        for u, v in edges:\n            adj[u][v] = 1\n            adj[v][u] = 1\n        return adj\n\n    adj_G = get_adj_matrix(edges_G, N)\n    adj_H = get_adj_matrix(edges_H, N)\n\n    def calculate_cost(adj_G, adj_H, A):\n        cost = 0\n        for i in range(N):\n            for j in range(i + 1, N):\n                if adj_G[i][j] != adj_H[i][j]:\n                    cost += A[i][j]\n        return cost\n\n    result = calculate_cost(adj_G, adj_H, A)\n    print(result)\n\nmin_cost_to_isomorphize()", "```python\nimport sys\nfrom itertools import permutations\n\ndef get_adj_matrix(N, edges):\n    adj_matrix = [[0] * N for _ in range(N)]\n    for u, v in edges:\n        u -= 1\n        v -= 1\n        adj_matrix[u][v] = 1\n        adj_matrix[v][u] = 1\n    return adj_matrix\n\ndef calculate_cost(G, H):\n    cost = 0\n    N = len(G)\n    for i in range(N):\n        for j in range(i + 1, N):\n            if G[i][j] != H[i][j]:\n                cost += abs(G[i][j] - H[i][j])\n    return cost\n\ndef is_isomorphic(G, H):\n    N = len(G)\n    return G == H\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs):\n    G = get_adj_matrix(N, edges_G)\n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        H = get_adj_matrix(N, edges_H)\n        for i in range(N):\n            for j in range(i + 1, N):\n                if G[i][j] != H[perm[i]][perm[j]]:\n                    H[perm[i]][perm[j]] = 1 - H[perm[i]][perm[j]]\n        if is_isomorphic(G, H):\n            min_cost = min(min_cost, calculate_cost(G, H))\n    \n    return min_cost\n\ndef main():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    M_H = int(data[index])\n    index += 1\n    edges_H = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    costs = [[int(data[index + i * N + j]) for j in range(N)] for i in range(N - 1)]\n    \n    result = min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    def next_int():\n        nonlocal index\n        value = int(data[index])\n        index += 1\n        return value\n\n    N = next_int()\n    M_G = next_int()\n    G_edges = [(next_int(), next_int()) for _ in range(M_G)]\n    M_H = next_int()\n    H_edges = [(next_int(), next_int()) for _ in range(M_H)]\n    A = [[next_int() for _ in range(N)] for _ in range(N)]\n\n    # Create adjacency matrices for G and H\n    adj_G = [[False] * N for _ in range(N)]\n    adj_H = [[False] * N for _ in range(N)]\n    for u, v in G_edges:\n        adj_G[u-1][v-1] = True\n        adj_G[v-1][u-1] = True\n    for u, v in H_edges:\n        adj_H[u-1][v-1] = True\n        adj_H[v-1][u-1] = True\n\n    # Initialize DP table\n    dp = [[[float('inf')] * (1 << N) for _ in range(N)] for _ in range(N)]\n    dp[0][0][0] = 0\n\n    # Fill DP table\n    for k in range(1 << N):\n        for i in range(N):\n            for j in range(i + 1, N):\n                if k & (1 << i) and k & (1 << j):\n                    if adj_G[i][j]:\n                        dp[i][j][k] = min(dp[i][j][k], dp[i][j][k ^ (1 << i) ^ (1 << j)])\n                    else:\n                        dp[i][j][k] = min(dp[i][j][k], dp[i][j][k ^ (1 << i) ^ (1 << j)] + A[i][j])\n\n    # Find the minimum cost to make H isomorphic to G\n    min_cost = float('inf')\n    for k in range(1 << N):\n        for i in range(N):\n            for j in range(i + 1, N):\n                if adj_G[i][j]:\n                    if adj_H[i][j]:\n                        min_cost = min(min_cost, dp[i][j][k])\n                    else:\n                        min_cost = min(min_cost, dp[i][j][k] + A[i][j])\n                else:\n                    if adj_H[i][j]:\n                        min_cost = min(min_cost, dp[i][j][k] + A[i][j])\n                    else:\n                        min_cost = min(min_cost, dp[i][j][k])\n\n    print(min_cost)\n\nmin_cost_to_isomorphic()", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(N):\n        row = []\n        for j in range(i + 1, N):\n            cost = int(data[index])\n            index += 1\n            row.append(cost)\n        A.append(row)\n    \n    # Adjacency matrix representation\n    adj_G = [[0] * N for _ in range(N)]\n    for u, v in edges_G:\n        adj_G[u][v] = 1\n        adj_G[v][u] = 1\n    \n    adj_H = [[0] * N for _ in range(N)]\n    for a, b in edges_H:\n        adj_H[a][b] = 1\n        adj_H[b][a] = 1\n    \n    # DP table initialization\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    # Iterate over all subsets of vertices\n    for mask in range(1 << N):\n        subset_size = bin(mask).count('1')\n        if subset_size == 0:\n            continue\n        \n        # Try adding/removing edges\n        for i in range(N):\n            for j in range(i + 1, N):\n                if mask & (1 << i) and mask & (1 << j):\n                    new_mask = mask ^ (1 << i) ^ (1 << j)\n                    if adj_G[i][j] != adj_H[i][j]:\n                        dp[new_mask] = min(dp[new_mask], dp[mask] + A[i][j])\n    \n    # Result is the minimum cost for the full set of vertices\n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()", "```python\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\ndef min_cost_to_isomorphic():\n    N = int(data[0])\n    M_G = int(data[1])\n    edges_G = [(int(data[2 + 2 * i]) - 1, int(data[3 + 2 * i]) - 1) for i in range(M_G)]\n    M_H = int(data[2 + 2 * M_G])\n    edges_H = [(int(data[3 + 2 * M_G + 2 * i]) - 1, int(data[4 + 2 * M_G + 2 * i]) - 1) for i in range(M_H)]\n    A = [[0] * N for _ in range(N)]\n    index = 2 + 2 * M_G + 2 * M_H\n    for i in range(N):\n        for j in range(i + 1, N):\n            A[i][j] = int(data[index])\n            A[j][i] = A[i][j]\n            index += 1\n    \n    # Calculate initial costs\n    initial_costs = [[0] * N for _ in range(N)]\n    for i in range(N):\n        for j in range(i + 1, N):\n            if (i, j) in edges_G:\n                if (i, j) not in edges_H:\n                    initial_costs[i][j] = A[i][j]\n                elif (j, i) not in edges_H:\n                    initial_costs[i][j] = A[i][j]\n            else:\n                if (i, j) in edges_H:\n                    initial_costs[i][j] = A[i][j]\n                elif (j, i) in edges_H:\n                    initial_costs[i][j] = A[i][j]\n    \n    # DP table\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    for mask in range(1 << N):\n        for i in range(N):\n            for j in range(i + 1, N):\n                if mask & (1 << i) and mask & (1 << j):\n                    new_mask = mask ^ (1 << i) ^ (1 << j)\n                    dp[new_mask] = min(dp[new_mask], dp[mask] + initial_costs[i][j])\n                elif not (mask & (1 << i)) and not (mask & (1 << j)):\n                    new_mask = mask | (1 << i) | (1 << j)\n                    dp[new_mask] = min(dp[new_mask], dp[mask] + initial_costs[i][j])\n    \n    return dp[(1 << N) - 1]\n\nprint(min_cost_to_isomorphic())\n```", "```python\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM_G = int(data[1])\nedges_G = [(int(data[2 + 2 * i]) - 1, int(data[3 + 2 * i]) - 1) for i in range(M_G)]\nM_H = int(data[2 + 2 * M_G])\nedges_H = [(int(data[3 + 2 * M_G + 2 * i]) - 1, int(data[4 + 2 * M_G + 2 * i]) - 1) for i in range(M_H)]\nA = [[int(data[5 + 2 * M_G + 2 * M_H + i * N + j]) for j in range(N)] for i in range(N)]\n\n# Create adjacency matrices for G and H\nadj_G = [[0] * N for _ in range(N)]\nadj_H = [[0] * N for _ in range(N)]\n\nfor u, v in edges_G:\n    adj_G[u][v] = 1\n    adj_G[v][u] = 1\n\nfor u, v in edges_H:\n    adj_H[u][v] = 1\n    adj_H[v][u] = 1\n\n# Calculate the difference in edges between G and H\nedge_diff = [[0] * N for _ in range(N)]\nfor u in range(N):\n    for v in range(u + 1, N):\n        edge_diff[u][v] = adj_G[u][v] - adj_H[u][v]\n        edge_diff[v][u] = edge_diff[u][v]\n\n# Compute the minimum cost to make the graphs isomorphic\nmin_cost = 0\nfor u in range(N):\n    for v in range(u + 1, N):\n        if edge_diff[u][v] != 0:\n            min_cost += abs(edge_diff[u][v]) * A[u][v]\n\nprint(min_cost)\n```", "```python\nimport sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorphic(G_edges, H_edges, costs):\n    N = len(costs)\n    \n    def cost_of_permutation(H_perm):\n        current_edges = set()\n        for i in range(N):\n            for j in range(i + 1, N):\n                if H_perm[i] < H_perm[j]:\n                    current_edges.add((H_perm[i], H_perm[j]))\n                else:\n                    current_edges.add((H_perm[j], H_perm[i]))\n        \n        cost = 0\n        for u, v in G_edges:\n            if (u, v) not in current_edges and (v, u) not in current_edges:\n                cost += costs[u - 1][v - 1]\n            elif (u, v) in current_edges and (v, u) in current_edges:\n                cost -= costs[u - 1][v - 1]\n        \n        return abs(cost)\n    \n    min_cost = float('inf')\n    for perm in permutations(range(1, N + 1)):\n        min_cost = min(min_cost, cost_of_permutation(perm))\n    \n    return min_cost\n\n# Read input\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM_G = int(data[1])\n\nG_edges = []\nindex = 2\nfor _ in range(M_G):\n    u = int(data[index])\n    v = int(data[index + 1])\n    G_edges.append((u, v))\n    index += 2\n\nM_H = int(data[index])\nindex += 1\n\nH_edges = []\nfor _ in range(M_H):\n    a = int(data[index])\n    b = int(data[index + 1])\n    H_edges.append((a, b))\n    index += 2\n\ncosts = []\nfor _ in range(N - 1):\n    row = list(map(int, data[index:index + N - 1]))\n    costs.append(row)\n    index += N - 1\n\n# Calculate and print the result\nprint(min_cost_to_isomorphic(G_edges, H_edges, costs))", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphize_graphs():\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n\n    M_G = int(data[index])\n    index += 1\n    edges_G = set()\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.add((min(u, v), max(u, v)))\n        index += 2\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = set()\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.add((min(a, b), max(a, b)))\n        index += 2\n\n    A = [[int(data[index + i * N + j]) for j in range(N)] for i in range(N - 1)]\n    index += N * (N - 1)\n\n    # Calculate the cost to make H isomorphic to G\n    cost = 0\n    for i in range(N):\n        for j in range(i + 1, N):\n            edge_exists_in_G = (i, j) in edges_G or (j, i) in edges_G\n            edge_exists_in_H = (i, j) in edges_H or (j, i) in edges_H\n\n            if edge_exists_in_G != edge_exists_in_H:\n                cost += A[i][j]\n\n    print(cost)\n\nif __name__ == '__main__':\n    min_cost_to_isomorphize_graphs()", "```python\nimport sys\nfrom itertools import permutations\nimport numpy as np\n\ndef is_isomorphic(G, H):\n    return np.array_equal(G, H)\n\ndef min_cost_to_isomorphic(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n    for perm in permutations(range(N)):\n        H_perm = np.zeros((N, N), dtype=int)\n        for i in range(N):\n            for j in range(i + 1, N):\n                H_perm[i, j] = A[perm[i] - 1, perm[j] - 1]\n                H_perm[j, i] = H_perm[i, j]\n        if is_isomorphic(G, H_perm):\n            cost = 0\n            for i in range(N):\n                for j in range(i + 1, N):\n                    if G[i, j] != H_perm[i, j]:\n                        cost += A[i, j]\n            min_cost = min(min_cost, cost)\n    return min_cost\n\ndef main():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G = np.zeros((N, N), dtype=int)\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        G[u, v] = 1\n        G[v, u] = 1\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    H = np.zeros((N, N), dtype=int)\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        H[a, b] = 1\n        H[b, a] = 1\n        index += 2\n    \n    A = []\n    for _ in range(N - 1):\n        row = list(map(int, data[index:index + N - 1]))\n        A.append(row)\n        index += N - 1\n    \n    A = np.array(A)\n    \n    result = min_cost_to_isomorphic(G, H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()", "```python\nimport sys\nfrom itertools import permutations\nimport networkx as nx\n\ndef parse_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G_edges = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    H_edges = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    \n    A = []\n    for i in range(N):\n        row = [int(data[index]) for _ in range(N - i - 1)]\n        A.append(row)\n        index += len(row)\n    \n    return N, M_G, G_edges, M_H, H_edges, A\n\ndef is_isomorphic(G, H):\n    return nx.is_isomorphic(G, H)\n\ndef min_cost_to_isomorphic(N, M_G, G_edges, M_H, H_edges, A):\n    G = nx.Graph()\n    G.add_edges_from(G_edges)\n    \n    min_cost = float('inf')\n    \n    for perm in permutations(range(1, N + 1)):\n        H_perm = nx.relabel_nodes(H, dict(zip(range(1, N + 1), perm)))\n        \n        if is_isomorphic(G, H_perm):\n            cost = sum(A[i][j] for i in range(N) for j in range(i + 1, N) if (perm[i], perm[j]) not in H.edges() != (i + 1, j + 1) in G.edges())\n            min_cost = min(min_cost, cost)\n    \n    return min_cost\n\nif __name__ == \"__main__\":\n    N, M_G, G_edges, M_H, H_edges, A = parse_input()\n    result = min_cost_to_isomorphic(N, M_G, G_edges, M_H, H_edges, A)\n    print(result)\n```", "```python\nfrom itertools import permutations\n\ndef is_isomorphic(G, H):\n    return G == H\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A):\n    G = [[0] * N for _ in range(N)]\n    for u, v in edges_G:\n        G[u-1][v-1] = 1\n        G[v-1][u-1] = 1\n    \n    H = [[0] * N for _ in range(N)]\n    for a, b in edges_H:\n        H[a-1][b-1] = 1\n        H[b-1][a-1] = 1\n    \n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        H_perm = [[0] * N for _ in range(N)]\n        for i in range(N):\n            for j in range(i + 1, N):\n                H_perm[i][j] = H[perm[i]][perm[j]]\n                H_perm[j][i] = H_perm[i][j]\n        \n        if is_isomorphic(G, H_perm):\n            current_cost = 0\n            for i in range(N):\n                for j in range(i + 1, N):\n                    if G[i][j] != H_perm[i][j]:\n                        current_cost += A[i][j] if G[i][j] == 0 else -A[i][j]\n            min_cost = min(min_cost, abs(current_cost))\n    \n    return min_cost\n\nif __name__ == \"__main__\":\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index])\n        index += 1\n        v = int(data[index])\n        index += 1\n        edges_G.append((u, v))\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index])\n        index += 1\n        b = int(data[index])\n        index += 1\n        edges_H.append((a, b))\n    \n    A = []\n    for i in range(N-1):\n        row = []\n        for j in range(i+1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n    \n    print(min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A))\n```", "```python\nimport sys\nfrom itertools import permutations\n\ndef is_isomorphic(G, H):\n    return G == H\n\ndef min_cost_to_isomorphic(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        H_perm = [[0] * N for _ in range(N)]\n        for i in range(N):\n            for j in range(i + 1, N):\n                H_perm[i][j] = A[perm[i]][perm[j]]\n                H_perm[j][i] = A[perm[j]][perm[i]]\n        \n        if is_isomorphic(G, H_perm):\n            cost = 0\n            for i in range(N):\n                for j in range(i + 1, N):\n                    if G[i][j] != H_perm[i][j]:\n                        cost += A[i][j]\n            min_cost = min(min_cost, cost)\n    \n    return min_cost if min_cost != float('inf') else 0\n\ndef main():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    \n    G = [[0] * N for _ in range(N)]\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        index += 2\n        G[u][v] = 1\n        G[v][u] = 1\n    \n    M_H = int(data[index])\n    index += 1\n    \n    H = [[0] * N for _ in range(N)]\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        index += 2\n        H[a][b] = 1\n        H[b][a] = 1\n    \n    A = []\n    for _ in range(N - 1):\n        A.append(list(map(int, data[index:index + N - 1])))\n        index += N - 1\n    \n    result = min_cost_to_isomorphic(G, H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\nimport sys\nfrom itertools import permutations\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    index = 0\n\n    N = int(data[index])\n    index += 1\n\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n\n    A = []\n    for i in range(N - 1):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n\n    return N, M_G, edges_G, M_H, edges_H, A\n\ndef adjacency_matrix(edges, N):\n    adj = [[0] * N for _ in range(N)]\n    for u, v in edges:\n        adj[u][v] = 1\n        adj[v][u] = 1\n    return adj\n\ndef is_isomorphic(adj_G, adj_H):\n    return adj_G == adj_H\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A):\n    adj_G = adjacency_matrix(edges_G, N)\n    min_cost = float('inf')\n\n    for perm in permutations(range(N)):\n        adj_H_perm = adjacency_matrix([(perm[a], perm[b]) for a, b in edges_H], N)\n        if is_isomorphic(adj_G, adj_H_perm):\n            cost = sum(A[i][j] for i in range(N) for j in range(i + 1, N) if adj_G[i][j] != adj_H_perm[i][j])\n            min_cost = min(min_cost, cost)\n\n    return min_cost\n\ndef main():\n    N, M_G, edges_G, M_H, edges_H, A = read_input()\n    result = min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G_edges = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        G_edges.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    H_edges = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        H_edges.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(N):\n        row = []\n        for j in range(i + 1, N):\n            cost = int(data[index])\n            index += 1\n            row.append(cost)\n        A.append(row)\n    \n    # Convert edge lists to adjacency matrices\n    G_adj = [[0] * N for _ in range(N)]\n    for u, v in G_edges:\n        G_adj[u][v] = 1\n        G_adj[v][u] = 1\n    \n    H_adj = [[0] * N for _ in range(N)]\n    for a, b in H_edges:\n        H_adj[a][b] = 1\n        H_adj[b][a] = 1\n    \n    # DP table to store the minimum cost to transform H into a graph isomorphic to G\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    for mask in range(1 << N):\n        for i in range(N):\n            if not (mask & (1 << i)):\n                continue\n            for j in range(i + 1, N):\n                if not (mask & (1 << j)):\n                    continue\n                new_mask = mask | (1 << i) | (1 << j)\n                if G_adj[i][j] != H_adj[i][j]:\n                    dp[new_mask] = min(dp[new_mask], dp[mask] + A[i][j])\n    \n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()", "```python\nimport sys\nfrom itertools import permutations\nimport numpy as np\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = [(int(data[index]) - 1, int(data[index + 1]) - 1) for _ in range(M_G)]\n    index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = [(int(data[index]) - 1, int(data[index + 1]) - 1) for _ in range(M_H)]\n    index += 2\n    \n    costs = []\n    for i in range(1, N):\n        row = [int(data[index]) for _ in range(i)]\n        costs.append(row)\n        index += len(row)\n    \n    return N, M_G, edges_G, M_H, edges_H, costs\n\ndef is_isomorphic(G, H, perm):\n    N = len(G)\n    H_perm = np.zeros((N, N), dtype=int)\n    for i in range(N):\n        for j in range(i + 1, N):\n            if H[perm[i]][perm[j]] == 1:\n                H_perm[i][j] = 1\n                H_perm[j][i] = 1\n    \n    return np.array_equal(G, H_perm)\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs):\n    G = np.zeros((N, N), dtype=int)\n    for u, v in edges_G:\n        G[u][v] = 1\n        G[v][u] = 1\n    \n    H = np.zeros((N, N), dtype=int)\n    for a, b in edges_H:\n        H[a][b] = 1\n        H[b][a] = 1\n    \n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        if is_isomorphic(G, H, perm):\n            current_cost = sum(costs[i][j] * (1 - H[perm[i]][perm[j]]) for i in range(N) for j in range(i + 1, N))\n            min_cost = min(min_cost, current_cost)\n    \n    return min_cost\n\ndef main():\n    N, M_G, edges_G, M_H, edges_H, costs = read_input()\n    result = min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "def min_cost_to_isomorphic_graphs():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    M_H = int(data[index])\n    index += 1\n    edges_H = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    A = [[int(data[index + i * N + j]) for j in range(N - 1)] for i in range(N - 1)]\n\n    def is_isomorphic(G, H):\n        from itertools import permutations\n        for perm in permutations(range(1, N + 1)):\n            new_H = set((perm[a], perm[b]) for a, b in H)\n            if new_H == set(G):\n                return True\n        return False\n\n    if is_isomorphic(edges_G, edges_H):\n        print(0)\n        return\n\n    def toggle_edges(H, i, j):\n        if (i, j) in H:\n            H.remove((i, j))\n        else:\n            H.add((i, j))\n\n    from heapq import heappop, heappush\n    heap = []\n    for i in range(1, N + 1):\n        for j in range(i + 1, N + 1):\n            if (i, j) in edges_G and (i, j) not in edges_H:\n                heappush(heap, (A[i - 1][j - 1], (i, j)))\n            elif (i, j) not in edges_G and (i, j) in edges_H:\n                heappush(heap, (A[i - 1][j - 1], (i, j)))\n\n    while heap:\n        cost, (i, j) = heappop(heap)\n        toggle_edges(edges_H, i, j)\n        if is_isomorphic(edges_G, edges_H):\n            print(cost)\n            return\n\nmin_cost_to_isomorphic_graphs()", "```python\nimport sys\nimport itertools\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    M_G = int(data[1])\n    edges_G = [(int(data[2 + 2 * i]) - 1, int(data[3 + 2 * i]) - 1) for i in range(M_G)]\n    \n    M_H = int(data[2 + 2 * M_G])\n    edges_H = [(int(data[3 + 2 * M_G + 2 * i]) - 1, int(data[4 + 2 * M_G + 2 * i]) - 1) for i in range(M_H)]\n    \n    A = []\n    index = 5 + 2 * M_G + 2 * M_H\n    for i in range(N):\n        row = []\n        for j in range(N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n    \n    return N, M_G, edges_G, M_H, edges_H, A\n\ndef is_isomorphic(adj_G, adj_H):\n    return adj_G == adj_H\n\ndef get_cost(adj_G, adj_H, A, perm):\n    cost = 0\n    for i in range(len(perm)):\n        for j in range(i + 1, len(perm)):\n            if adj_G[i][j] != adj_H[perm[i]][perm[j]]:\n                cost += A[i][j]\n    return cost\n\ndef find_min_cost(N, M_G, edges_G, M_H, edges_H, A):\n    adj_G = [[0] * N for _ in range(N)]\n    for u, v in edges_G:\n        adj_G[u][v] = 1\n        adj_G[v][u] = 1\n    \n    min_cost = float('inf')\n    for perm in itertools.permutations(range(N)):\n        adj_H = [[0] * N for _ in range(N)]\n        for u, v in edges_H:\n            adj_H[u][v] = 1\n            adj_H[v][u] = 1\n        \n        for i in range(N):\n            for j in range(i + 1, N):\n                if adj_H[i][j] != adj_G[perm[i]][perm[j]]:\n                    adj_H[i][j] = 1 - adj_H[i][j]\n        \n        if is_isomorphic(adj_G, adj_H):\n            cost = get_cost(adj_G, adj_H, A, perm)\n            if cost < min_cost:\n                min_cost = cost\n    \n    return min_cost\n\ndef main():\n    N, M_G, edges_G, M_H, edges_H, A = read_input()\n    min_cost = find_min_cost(N, M_G, edges_G, M_H, edges_H, A)\n    print(min_cost)\n\nif __name__ == \"__main__\":\n    main()\n```", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic(N", "```python\nimport sys\nfrom itertools import permutations\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    \n    A = []\n    for i in range(1, N):\n        row = [int(data[index]) for _ in range(i, N)]\n        index += len(row)\n        A.append(row)\n    \n    return N, M", "import sys\nfrom itertools import permutations\nfrom collections import defaultdict\n\ndef min_cost_to_isomorph(G, H, A):\n    N = len(G)\n    M_G = len(G[0]) // 2\n    M_H = len(H[0]) // 2\n\n    # Convert adjacency lists to sets for easier manipulation\n    G_set = [set() for _ in range(N)]\n    for u, v in zip(G[::2], G[1::2]):\n        G_set[u-1].add(v-1)\n        G_set[v-1].add(u-1)\n\n    H_set = [set() for _ in range(N)]\n    for a, b in zip(H[::2], H[1::2]):\n        H_set[a-1].add(b-1)\n        H_set[b-1].add(a-1)\n\n    # Initialize DP table\n    dp = [[float('inf')] * (1 << N) for _ in range(M_G + 1)]\n    dp[0][0] = 0\n\n    # Fill DP table\n    for g_edge_count in range(1, M_G + 1):\n        for h_state in range(1 << N):\n            for perm in permutations(range(N)):\n                new_h_state = 0\n                for i in range(N):\n                    for j in range(i+1, N):\n                        if (perm[i], perm[j]) in G_set or (perm[j], perm[i]) in G_set:\n                            new_h_state |= (1 << j)\n\n                if new_h_state == h_state:\n                    continue\n\n                cost = sum(A[i][j] for i in range(N) for j in range(i+1, N) if (h_state & (1 << i)) and (h_state & (1 << j)) != (new_h_state & (1 << i)) and (new_h_state & (1 << j)))\n                dp[g_edge_count][h_state] = min(dp[g_edge_count][h_state], dp[g_edge_count - 1][new_h_state] + cost)\n\n    return dp[M_G][(1 << N) - 1]\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G = []\n    for _ in range(M_G * 2):\n        G.append(int(data[index]))\n        index += 1\n\n    M_H = int(data[index])\n    index += 1\n    H = []\n    for _ in range(M_H * 2):\n        H.append(int(data[index]))\n        index += 1\n\n    A = []\n    for _ in range(N):\n        row = []\n        for _ in range(N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n\n    result = min_cost_to_isomorph(G, H, A)\n    print(result)", "```python\nimport sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = [[int(data[index + j * (N - 1) + i]) for i in range(1, N)] for j in range(N - 1)]\n    index += (N - 1) * N // 2\n    \n    # Function to get the complement of a set\n    def complement(mask):\n        return (1 << N) - 1 - mask\n    \n    # DP table initialization\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    # Iterate over all subsets\n    for mask in range(1 << N):\n        # Get the list of vertices in the current subset\n        vertices = [i for i in range(N) if mask & (1 << i)]\n        \n        # If the subset is empty, skip it\n        if not vertices:\n            continue\n        \n        # Calculate the cost to connect the vertices in the subset\n        for i in range(len(vertices)):\n            for j in range(i + 1, len(vertices)):\n                u, v = vertices[i], vertices[j]\n                if (u, v) in edges_G and (v, u) in edges_G:\n                    dp[mask] = min(dp[mask], dp[mask ^ (1 << u) ^ (1 << v)])\n                elif (u, v) not in edges_G and (v, u) not in edges_G:\n                    dp[mask] = min(dp[mask], dp[mask ^ (1 << u) ^ (1 << v)] + A[u][v])\n    \n    # Print the minimum cost to make H isomorphic to G\n    print(dp[(1 << N) - 1])\n\n# Call the function\nmin_cost_to_isomorphic()", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    def next_int():\n        nonlocal index\n        result = int(data[index])\n        index += 1\n        return result\n    \n    N = next_int()\n    M_G = next_int()\n    G_edges = []\n    for _ in range(M_G):\n        u = next_int() - 1\n        v = next_int() - 1\n        G_edges.append((u, v))\n    \n    M_H = next_int()\n    H_edges = []\n    for _ in range(M_H):\n        a = next_int() - 1\n        b = next_int() - 1\n        H_edges.append((a, b))\n    \n    A = [[next_int() for _ in range(N)] for _ in range(N)]\n    \n    # Create adjacency lists for G and H\n    adj_G = [[] for _ in range(N)]\n    for u, v in G_edges:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n    \n    adj_H = [[] for _ in range(N)]\n    for a, b in H_edges:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n    \n    # Function to check if two sets of edges are equal\n    def edges_equal(edges1, edges2):\n        edges_set1 = set(tuple(sorted(e)) for e in edges1)\n        edges_set2 = set(tuple(sorted(e)) for e in edges2)\n        return edges_set1 == edges_set2\n    \n    # Dynamic programming to find the minimum cost\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    for mask in range(1 << N):\n        for i in range(N):\n            for j in range(i + 1, N):\n                bit = 1 << i | 1 << j\n                if mask & bit == bit:\n                    if (i, j) in adj_G and (j, i) in adj_G:\n                        if (i, j) not in adj_H and (j, i) not in adj_H:\n                            dp[mask] = min(dp[mask], dp[mask ^ bit] + A[i][j])\n                    elif (i, j) not in adj_G and (j, i) not in adj_G:\n                        if (i, j) in adj_H and (j, i) in adj_H:\n                            dp[mask] = min(dp[mask], dp[mask ^ bit] + A[i][j])\n    \n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()\n```", "def min_cost_to_isomorphize_graphs():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G_edges = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n    M_H = int(data[index])\n    index += 1\n    H_edges = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n    costs = [[int(data[index + i * N + j]) for j in range(N - i)] for i in range(N - 1)]\n\n    # Convert edge lists to adjacency matrices for easier manipulation\n    G_adj = [[0] * N for _ in range(N)]\n    for u, v in G_edges:\n        G_adj[u - 1][v - 1] = 1\n        G_adj[v - 1][u - 1] = 1\n\n    H_adj = [[0] * N for _ in range(N)]\n    for u, v in H_edges:\n        H_adj[u - 1][v - 1] = 1\n        H_adj[v - 1][u - 1] = 1\n\n    # Function to calculate the cost to transform H into G\n    def transform_cost(H, G):\n        total_cost = 0\n        for i in range(N):\n            for j in range(i + 1, N):\n                if H[i][j] != G[i][j]:\n                    total_cost += costs[i][j]\n        return total_cost\n\n    # Calculate the initial cost to transform H into G\n    initial_cost = transform_cost(H_adj, G_adj)\n\n    # Try all permutations of H to find the minimum cost\n    from itertools import permutations\n    min_cost = float('inf')\n    for perm in permutations(range(1, N + 1)):\n        permuted_H_adj = [[H_adj[perm[i] - 1][perm[j] - 1] for j in range(N)] for i in range(N)]\n        current_cost = transform_cost(permuted_H_adj, G_adj)\n        min_cost = min(min_cost, current_cost)\n\n    print(min_cost)\n\n# Call the function to execute the code\nmin_cost_to_isomorphize_graphs()", "```python\nimport sys\nfrom collections import defaultdict\nfrom functools import lru_cache\n\ndef min_cost_to_isomorphic():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    costs = [[int(data[index + i * N + j]) for j in range(N)] for i in range(N)]\n    \n    # Convert edges to adjacency lists\n    adj_G = defaultdict(list)\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n    \n    adj_H = defaultdict(list)\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n    \n    @lru_cache(None)\n    def dfs(state, perm):\n        if state == 0:\n            return 0\n        \n        min_cost = float('inf')\n        for next_perm in permutations(perm):\n            next_state = 0\n            for i in range(N):\n                if next_perm[i] != perm[i]:\n                    next_state |= (1 << i)\n            \n            cost = 0\n            for i in range(N):\n                for j in range(i + 1, N):\n                    if (next_state & (1 << i)) and (next_state & (1 << j)):\n                        if not adj_G[next_perm[i]].__contains__(next_perm[j]):\n                            cost += costs[i][j]\n                    elif not (next_state & (1 << i)) and not (next_state & (1 << j)):\n                        if adj_G[next_perm[i]].__contains__(next_perm[j]):\n                            cost += costs[i][j]\n            \n            min_cost = min(min_cost, cost + dfs(next_state, next_perm))\n        \n        return min_cost\n    \n    initial_state = 0\n    for i in range(N):\n        initial_state |= (1 << i)\n    \n    result = dfs(initial_state, tuple(range(N)))\n    print(result)\n\nmin_cost_to_isomorphic()", "import sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorphize(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n\n    # Check all permutations of vertices in H\n    for perm in permutations(range(1, N+1)):\n        current_cost = 0\n        # Calculate the cost to make H isomorphic to G under the current permutation\n        for i in range(N-1):\n            for j in range(i+1, N):\n                if G[i][j] != H[perm[i]-1][perm[j]-1]:\n                    current_cost += A[i][j]\n        # Update the minimum cost if the current cost is lower\n        min_cost = min(min_cost, current_cost)\n\n    return min_cost\n\n# Read input from stdin\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM_G = int(data[1])\nG = [[0]*N for _ in range(N)]\nindex = 2\nfor _ in range(M_G):\n    u = int(data[index])-1\n    v = int(data[index+1])-1\n    G[u][v] = G[v][u] = 1\n    index += 2\n\nM_H = int(data[index])\nH = [[0]*N for _ in range(N)]\nindex += 1\nfor _ in range(M_H):\n    a = int(data[index])-1\n    b = int(data[index+1])-1\n    H[a][b] = H[b][a] = 1\n    index += 2\n\nA = []\nfor i in range(N-1):\n    row = list(map(int, data[index:index+N-i]))\n    A.append(row)\n    index += N-i\n\n# Calculate and print the minimum cost\nprint(min_cost_to_isomorphize(G, H, A))", "```python\nimport sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(1, N):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n    \n    # Convert edges to adjacency list representation\n    adj_G = defaultdict(list)\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n    \n    adj_H = defaultdict(list)\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n    \n    # Initialize DP table\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    # Iterate over all possible states of H\n    for state in range(1 << N):\n        current_edges = set()\n        for i in range(N):\n            for j in range(i + 1, N):\n                if state & (1 << i) and state & (1 << j):\n                    current_edges.add((i, j))\n        \n        # Check if current state can be transformed to match G\n        for u in range(N):\n            for v in range(u + 1, N):\n                if (u, v) in adj_G and (v, u) in adj_G:\n                    if ((u, v) not in current_edges and (v, u) not in current_edges) or \\\n                       ((u, v) in current_edges and (v, u) in current_edges):\n                        continue\n                    new_state = state\n                    if (u, v) not in current_edges:\n                        new_state |= (1 << u) | (1 << v)\n                    else:\n                        new_state &= ~(1 << u) & ~(1 << v)\n                    \n                    dp[new_state] = min(dp[new_state], dp[state] + A[u][v])\n    \n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()"]}
