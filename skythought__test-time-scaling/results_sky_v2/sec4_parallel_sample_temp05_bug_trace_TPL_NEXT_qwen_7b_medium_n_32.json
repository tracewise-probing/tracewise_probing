tr)):\n                swapped_num_str = num_str[:i] + num_str[j] + num_str[i + 1:j] + num_str[i] + num_str[j + 1:]\n                swapped.add(int(swapped_num_str))\n        return swapped\n\n    num_to_swapped = {}\n    for num in nums:\n        num_str = str(num)\n        if num_str not in num_to_swapped:\n            num_to_swapped[num_str] = get_swapped_numbers(num_str)\n\n    count = 0\n    for i in range(len(nums)):\n        for j in range(i + 1, len(nums)):\n            if nums[j] in num_to_swapped[str(nums[i])]:\n                count += 1\n            if nums[i] in num_to_swapped[str(nums[j])]:\n                count += 1\n\n    return count\n```", "```python\ndef almost_equal_pairs(nums):\n    n = len(nums)\n    count = 0\n    \n    for i in range(n):\n        for j in range(i + 1, n):\n            num1 = str(nums[i])\n            num2 = str(nums[j])\n            \n            if len(num1) != len(num2):\n                continue\n            \n            digit_count1 = [0] * 10\n            digit_count2 = [0] * 10\n            \n            for d in num1:\n                digit_count1[int(d)] += 1\n            \n            for d in num2:\n                digit_count2[int(d)] += 1\n            \n            diff = 0\n            for k in range(10):\n                if digit_count1[k] != digit_count2[k]:\n                    diff += abs(digit_count1[k] - digit_count2[k])\n            \n            if diff <= 2:\n                count += 1\n    \n    return count\n```", "```python\ndef count_almost_equal_pairs(nums):\n    n = len(nums)\n    num_set = set(nums)\n    count = 0\n    \n    for num in nums:\n        str_num = str(num)\n        length = len(str_num)\n        \n        # Generate all possible swaps\n        for i in range(length):\n            for j in range(i + 1, length):\n                swapped_str = list(str_num)\n                swapped_str[i], swapped_str[j] = swapped_str[j], swapped_str[i]\n                swapped_num = int(''.join(swapp