                grid[R_q][j] = False\n                    break\n            for j in range(C_q + 1, W):\n                if grid[R_q][j]:\n                    grid[R_q][j] = False\n                    break\n    \n    remaining_walls = sum(row.count(True) for row in grid)\n    print(remaining_walls)\n\nmain()\n```", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    H = int(data[index])\n    index += 1\n    W = int(data[index])\n    index += 1\n    Q = int(data[index])\n    index += 1\n    \n    walls = set()\n    for i in range(H):\n        for j in range(W):\n            walls.add((i, j))\n    \n    for _ in range(Q):\n        R = int(data[index]) - 1\n        index += 1\n        C = int(data[index]) - 1\n        index += 1\n        \n        if (R, C) in walls:\n            walls.remove((R, C))\n        else:\n            for dr, dc in [(-1, 0), (1, 0), (0, -1), (0, 1)]:\n                r, c = R, C\n                while True:\n                    r += dr\n                    c += dc\n                    if 0 <= r < H and 0 <= c < W and (r, c) in walls:\n                        walls.remove((r, c))\n                        break\n    \n    print(len(walls))\n\nif __name__ == \"__main__\":\n    main()", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    H = int(data[index])\n    W = int(data[index + 1])\n    Q = int(data[index + 2])\n    index += 3\n    \n    grid = [[True] * W for _ in range(H)]\n    \n    for _ in range(Q):\n        R = int(data[index]) - 1\n        C = int(data[index + 1]) - 1\n        index += 2\n        \n        if grid[R][C]:\n            grid[R][C] = False\n        else:\n            # Check up\n            for i in range(R - 1, -1, -1):\n                if grid[i][C]:\n                    grid[i][C] = False\n                    break\n            \n            # Check down\n            for i in range(R + 1, H):\n                if grid[i][C]:\n                    grid[i][C] = False\n                    break\n            \n            # Check left\n            for j in range(C - 1, -1, -1):\n                if grid[R][j]:\n                    grid[R][j] = False\n                    break\n            \n            # Check right\n            for j in range(C + 1, W):\n                if grid[R][j]:\n                    grid[R][j] = False\n                    break\n    \n    remaining_walls = sum(row.count(True) for row in grid)\n    print(remaining_walls)\n\nmain()", "```python\ndef count_remaining_walls():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n    \n    # Initialize the grid with walls\n    grid = [[True] * W for _ in range(H)]\n    rows = [set(range(W)) for _ in range(H)]\n    cols = [set(range(H)) for _ in range(W)]\n    \n    index = 3\n    for _ in range(Q):\n        R_q = int(data[index]) - 1\n        C_q = int(data[index + 1]) - 1\n        index += 2\n        \n        if grid[R_q][C_q]:\n            # Destroy the wall at (R_q, C_q)\n            grid[R_q][C_q] = False\n            rows[R_q].remove(C_q)\n            cols[C_q].remove(R_q)\n        else:\n            # Destroy the first walls in each direction\n            if R_q > 0 and R_q - 1 in rows[R_q]:\n                grid[R_q - 1][C_q] = False\n                rows[R_q - 1].remove(C_q)\n                cols[C_q].remove(R_q - 1)\n            if R_q < H - 1 and R_q + 1 in rows[R_q]:\n                grid[R_q + 1][C_q] = False\n                rows[R_q + 1].remove(C_q)\n                cols[C_q].remove(R_q + 1)\n            if C_q > 0 and C_q - 1 in cols[C_q]:\n                grid[R_q][C_q - 1] = False\n                rows[R_q].remove(C_q - 1)\n                cols[C_q - 1].remove(R_q)\n            if C_q < W - 1 and C_q + 1 in cols[C_q]:\n                grid[R_q][C_q + 1] = False\n                rows[R_q].remove(C_q + 1)\n                cols[C_q + 1].remove(R_q)\n    \n    # Count the remaining walls\n    remaining_walls = sum(row.count(True) for row in grid)\n    print(remaining_walls)\n\ncount_remaining_walls()", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n\n    walls = {(i, j) for i in range(1, H + 1) for j in range(1, W + 1)}\n    above = {j: [] for j in range(1, W + 1)}\n    below = {j: [] for j in range(1, W + 1)}\n    left = {i: [] for i in range(1, H + 1)}\n    right = {i: [] for i in range(1, H + 1)}\n\n    for i in range(Q):\n        r = int(data[2 * i + 3])\n        c = int(data[2 * i + 4])\n\n        if (r, c) in walls:\n            walls.remove((r, c))\n        else:\n            for j in range(c - 1, 0, -1):\n                if (r, j) in walls:\n                    walls.remove((r, j))\n                    break\n            for j in range(c + 1, W + 1):\n                if (r, j) in walls:\n                    walls.remove((r, j))\n                    break\n            for i in range(r - 1, 0, -1):\n                if (i, c) in walls:\n                    walls.remove((i, c))\n                    break\n            for i in range(r + 1, H + 1):\n                if (i, c) in walls:\n                    walls.remove((i, c))\n                    break\n\n    print(len(walls))\n\nif __name__ == '__main__':\n    main()", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    H = int(data[index])\n    W = int(data[index + 1])\n    Q = int(data[index + 2])\n    index += 3\n    \n    grid = [[True] * W for _ in range(H)]\n    \n    for _ in range(Q):\n        R_q = int(data[index]) - 1\n        C_q = int(data[index + 1]) - 1\n        index += 2\n        \n        if grid[R_q][C_q]:\n            grid[R_q][C_q] = False\n        else:\n            # Destroy walls up, down, left, and right\n            for r in range(R_q - 1, -1, -1):\n                if grid[r][C_q]:\n                    grid[r][C_q] = False\n                    break\n            for r in range(R_q + 1, H):\n                if grid[r][C_q]:\n                    grid[r][C_q] = False\n                    break\n            for c in range(C_q - 1, -1, -1):\n                if grid[R_q][c]:\n                    grid[R_q][c] = False\n                    break\n            for c in range(C_q + 1, W):\n                if grid[R_q][c]:\n                    grid[R_q][c] = False\n                    break\n    \n    remaining_walls = sum(row.count(True) for row in grid)\n    print(remaining_walls)\n\nmain()", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n\n    # Union-Find structure\n    parent = [i for i in range(H * W + 1)]\n    rank = [0] * (H * W + 1)\n\n    def find(x):\n        if parent[x] != x:\n            parent[x] = find(parent[x])\n        return parent[x]\n\n    def union(x, y):\n        rootX = find(x)\n        rootY = find(y)\n        if rootX != rootY:\n            if rank[rootX] > rank[rootY]:\n                parent[rootY] = rootX\n            elif rank[rootX] < rank[rootY]:\n                parent[rootX] = rootY\n            else:\n                parent[rootY] = rootX\n                rank[rootX] += 1\n\n    # Initialize walls\n    for i in range(1, H * W + 1):\n        union(i, i - 1)  # left\n        union(i, i + 1)  # right\n        union(i, i - W)  # up\n        union(i, i + W)  # down\n\n    # Process queries\n    index = 3\n    for _ in range(Q):\n        R = int(data[index]) - 1\n        C = int(data[index + 1]) - 1\n        index += 2\n        pos = R * W + C + 1\n\n        # If there is no wall, destroy the first walls in all directions\n        if find(pos) != find(pos - 1):  # left\n            union(pos, pos - 1)\n        if find(pos) != find(pos + 1):  # right\n            union(pos, pos + 1)\n        if find(pos) != find(pos - W):  # up\n            union(pos, pos - W)\n        if find(pos) != find(pos + W):  # down\n            union(pos, pos + W)\n\n    # Count remaining walls\n    remaining_walls = len(set(find(i) for i in range(1, H * W + 1)))\n    print(remaining_walls)\n\nif __name__ == \"__main__\":\n    main()", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n\n    grid = [[True] * W for _ in range(H)]\n    index = 3\n\n    def destroy_wall(r, c):\n        nonlocal grid\n        if grid[r][c]:\n            grid[r][c] = False\n        else:\n            # Look up\n            for i in range(r - 1, -1, -1):\n                if grid[i][c]:\n                    grid[i][c] = False\n                    break\n            # Look down\n            for i in range(r + 1, H):\n                if grid[i][c]:\n                    grid[i][c] = False\n                    break\n            # Look left\n            for j in range(c - 1, -1, -1):\n                if grid[r][j]:\n                    grid[r][j] = False\n                    break\n            # Look right\n            for j in range(c + 1, W):\n                if grid[r][j]:\n                    grid[r][j] = False\n                    break\n\n    for _ in range(Q):\n        r = int(data[index]) - 1\n        c = int(data[index + 1]) - 1\n        destroy_wall(r, c)\n        index += 2\n\n    remaining_walls = sum(row.count(True) for row in grid)\n    print(remaining_walls)\n\nif __name__ == '__main__':\n    main()", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n    \n    grid = [[True] * W for _ in range(H)]\n    rows = [sorted(range(W)) for _ in range(H)]\n    cols = [sorted(range(H)) for _ in range(W)]\n    \n    index = 3\n    for _ in range(Q):\n        R = int(data[index]) - 1\n        C = int(data[index + 1]) - 1\n        index += 2\n        \n        if grid[R][C]:\n            grid[R][C] = False\n            rows[R].remove(C)\n            cols[C].remove(R)\n        else:\n            # Find the first wall in the row above\n            if R > 0 and rows[R-1]:\n                i = rows[R-1][0]\n                grid[i][C] = False\n                rows[i].remove(C)\n                cols[C].remove(i)\n            \n            # Find the first wall in the row below\n            if R < H-1 and rows[R+1]:\n                i = rows[R+1][0]\n                grid[i][C] = False\n                rows[i].remove(C)\n                cols[C].remove(i)\n            \n            # Find the first wall in the column to the left\n            if C > 0 and cols[C-1]:\n                j = cols[C-1][0]\n                grid[R][j] = False\n                rows[R].remove(j)\n                cols[j].remove(R)\n            \n            # Find the first wall in the column to the right\n            if C < W-1 and cols[C+1]:\n                j = cols[C+1][0]\n                grid[R][j] = False\n                rows[R].remove(j)\n                cols[j].remove(R)\n    \n    remaining_walls = sum(sum(row) for row in grid)\n    print(remaining_walls)\n\nmain()", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n\n    walls = set()\n    for i in range(H * W):\n        walls.add((i // W, i % W))\n\n    for i in range(Q):\n        r = int(data[3 + 2 * i]) - 1\n        c = int(data[4 + 2 * i]) - 1\n\n        if (r, c) in walls:\n            walls.remove((r, c))\n        else:\n            directions = [(-1, 0), (1, 0), (0, -1), (0, 1)]\n            for dr, dc in directions:\n                x, y = r, c\n                while 0 <= x + dr < H and 0 <= y + dc < W:\n                    x += dr\n                    y += dc\n                    if (x, y) in walls:\n                        walls.remove((x, y))\n                        break\n\n    print(len(walls))\n\nmain()", "def solve_problem():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n    queries = [(int(data[3 + 2 * i]), int(data[4 + 2 * i])) for i in range(Q)]\n    grid = [[1] * W for _ in range(H)]\n    directions = [(-1, 0), (1, 0), (0, -1), (0, 1)]\n    for R, C in queries:\n        R -= 1\n        C -= 1\n        if grid[R][C] == 0:\n            for d in directions:\n                r, c = R + d[0], C + d[1]\n                while 0 <= r < H and 0 <= c < W and grid[r][c] == 0:\n                    r += d[0]\n                    c += d[1]\n                if 0 <= r < H and 0 <= c < W and grid[r][c] == 1:\n                    grid[r][c] = 0\n        else:\n            grid[R][C] = 0\n    print(sum(sum(row) for row in grid))\nsolve_problem()", "```python\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    H = int(data[index])\n    W = int(data[index + 1])\n    Q = int(data[index + 2])\n    index += 3\n    \n    # Initialize sets to keep track of walls in each row and column\n    walls_row = [set(range(W)) for _ in range(H)]\n    walls_col = [set(range(H)) for _ in range(W)]\n    \n    total_walls = H * W\n    \n    for _ in range(Q):\n        R = int(data[index]) - 1\n        C = int(data[index + 1]) - 1\n        index += 2\n        \n        if R in walls_row[C] and C in walls_col[R]:\n            walls_row[C].remove(R)\n            walls_col[R].remove(C)\n            total_walls -= 1\n        elif R in walls_row[C]:\n            while R > 0 and C in walls_col[R - 1]:\n                walls_col[R - 1].remove(C)\n                walls_row[C].remove(R - 1)\n                total_walls -= 1\n                R -= 1\n        elif C in walls_col[R]:\n            while C > 0 and R in walls_row[C - 1]:\n                walls_row[C - 1].remove(R)\n                walls_col[R].remove(C - 1)\n                total_walls -= 1\n                C -= 1\n    \n    print(total_walls)\n\nmain()\n```", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    H = int(data[0])\n    W = int(data[1])\n    Q = int(data[2])\n\n    # Initialize the grid and sets to keep track of walls\n    grid = [[True] * W for _ in range(H)]\n    rows = [set() for _ in range(H)]\n    cols = [set() for _ in range(W)]\n\n    index = 3\n    for _ in range(Q):\n        R = int(data[index]) - 1\n        C = int(data[index + 1]) - 1\n        index += 2\n\n        if grid[R][C]:\n            # Destroy the wall at (R, C)\n            grid[R][C] = False\n            rows[R].remove(C)\n            cols[C].remove(R)\n        else:\n            # Find the first wall in the specified directions\n            for dr, dc in [(-1, 0), (1, 0), (0, -1), (0, 1)]:\n                r, c = R + dr, C + dc\n                while 0 <= r < H and 0 <= c < W and not grid[r][c]:\n                    r += dr\n                    c += dc\n                if 0 <= r < H and 0 <= c < W and grid[r][c]:\n                    grid[r][c] = False\n                    rows[r].remove(c)\n                    cols[c].remove(r)\n\n    # Count the remaining walls\n    remaining_walls = sum(len(row) for row in rows)\n    print(remaining_walls)\n\nmain()"]}
{"task_id": "abc365_c", "codes": ["def max_subsidy_limit(N, M, A):\n    def is_feasible(x):\n        return sum(min(x, ai) for ai in A) <= M\n\n    left, right = 0, max(A)\n    while left < right:\n        mid = (left + right + 1) // 2\n        if is_feasible(mid):\n            left = mid\n        else:\n            right = mid - 1\n\n    if is_feasible(left + 1):\n        return left + 1\n    elif is_feasible(left):\n        return left\n    else:\n        return 'infinite'\n\nimport sys\ninput = sys.stdin.read\ndata = input().split()\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    def can_subsidize(limit):\n        return sum(min(limit, ai) for ai in A) <= M\n    \n    if sum(A) <= M:\n        return \"infinite\"\n    \n    low, high = 0, max(A)\n    while low < high:\n        mid = (low + high + 1) // 2\n        if can_subsidize(mid):\n            low = mid\n        else:\n            high = mid - 1\n    \n    return low\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    A.sort()\n    \n    if M >= sum(A):\n        return \"infinite\"\n    \n    left, right = 0, max(A)\n    \n    while left < right:\n        mid = (left + right + 1) // 2\n        total_subsidy = sum(min(mid, ai) for ai in A)\n        \n        if total_subsidy <= M:\n            left = mid\n        else:\n            right = mid - 1\n    \n    return left\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    A.sort()\n    \n    def can_subsidize(x):\n        total_subsidy = 0\n        for ai in A:\n            total_subsidy += min(x, ai)\n            if total_subsidy > M:\n                return False\n        return True\n    \n    low, high = 0, A[-1]\n    while low < high:\n        mid = (low + high + 1) // 2\n        if can_subsidize(mid):\n            low = mid\n        else:\n            high = mid - 1\n    \n    if can_subsidize(low):\n        return low\n    else:\n        return \"infinite\"\n\n# Example usage:\n# import sys\n# input = sys.stdin.read\n# data = input().split()\n# N = int(data[0])\n# M = int(data[1])\n# A = list(map(int, data[2:]))\n\n# result = max_subsidy_limit(N, M, A)\n# print(result)", "def max_subsidy_limit(N, M, A):\n    def can_subsidize(x):\n        total_cost = 0\n        for ai in A:\n            total_cost += min(x, ai)\n            if total_cost > M:\n                return False\n        return True\n    \n    left, right = 0, max(A)\n    result = -1\n    \n    while left <= right:\n        mid = (left + right) // 2\n        if can_subsidize(mid):\n            result = mid\n            left = mid + 1\n        else:\n            right = mid - 1\n    \n    return result if result != -1 else \"infinite\"\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    def is_feasible(x):\n        return sum(min(x, a) for a in A) <= M\n\n    low, high = 0, max(A)\n    while low < high:\n        mid = (low + high + 1) // 2\n        if is_feasible(mid):\n            low = mid\n        else:\n            high = mid - 1\n\n    if is_feasible(low):\n        return low\n    else:\n        return 'infinite'\n\n# Example usage:\n# N = 4\n# M = 8\n# A = [1, 3, 2, 4]\n# print(max_subsidy_limit(N, M, A))\n\n# N = 3\n# M = 20\n# A = [5, 3, 2]\n# print(max_subsidy_limit(N, M, A))\n\n# N = 10\n# M = 23\n# A = [2, 5, 6, 5, 2, 1, 7, 9, 7, 2]\n# print(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    A.sort()\n    \n    def can_satisfy_budget(x):\n        total_cost = 0\n        for ai in A:\n            total_cost += min(x, ai)\n            if total_cost > M:\n                return False\n        return True\n    \n    left, right = 0, max(A)\n    while left < right:\n        mid = (left + right + 1) // 2\n        if can_satisfy_budget(mid):\n            left = mid\n        else:\n            right = mid - 1\n    \n    if can_satisfy_budget(left):\n        return left\n    elif can_satisfy_budget(left - 1):\n        return left - 1\n    else:\n        return \"infinite\"\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    if sum(A) <= M:\n        return \"infinite\"\n    \n    low, high = 0, max(A)\n    \n    while low < high:\n        mid = (low + high + 1) // 2\n        total_subsidy = sum(min(mid, a) for a in A)\n        \n        if total_subsidy <= M:\n            low = mid\n        else:\n            high = mid - 1\n    \n    return low\n\n# Read input\nN, M = map(int, input().split())\nA = list(map(int, input().split()))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "d", "def max_subsidy_limit(N, M, A):\n    A.sort()\n    if sum(A) <= M:\n        return \"infinite\"\n    \n    left, right = 0, A[-1]\n    while left < right:\n        mid = (left + right + 1) // 2\n        total_subsidy = sum(min(mid, a) for a in A)\n        if total_subsidy <= M:\n            left = mid\n        else:\n            right = mid - 1\n    \n    return left\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM = int(data[1])\nA = list(map(int, data[2:]))\n\n# Print the result\nprint(max_subsidy_limit(N, M, A))", "def max_subsidy_limit(N, M, A):\n    def can_subsidize(limit):\n        total_cost = sum(min(limit, ai) for ai i