)\n    q = int(data[1])\n    s = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[2 + 2 * i + 1]) for i in range(q)]\n    \n    def get_initial_count():\n        return count_abc_substrings(s)\n    \n    def apply_query(x, c):\n        nonlocal s\n        old_char = s[x]\n        s = s[:x] + c + s[x+1:]\n        \n        # Check the three possible substrings around the changed position\n        count_change = 0\n        if x > 0 and s[x-1:x+2] == 'ABC':\n            count_change -= 1\n        if x < n - 2 and s[x:x+3] == 'ABC':\n            count_change += 1\n        if x < n - 2 and s[x+1:x+4] == 'ABC':\n            count_change += 1\n        \n        return count_change\n    \n    initial_count = get_initial_count()\n    current_count = initial_count\n    \n    for x, c in queries:\n        count_change = apply_query(x, c)\n        current_count += count_change\n        print(current_count)\n\nprocess_queries()\n```", "```python\ndef count_abc_substrings(s):\n    n = len(s)\n    count = 0\n    \n    # Count initial occurrences of \"ABC\"\n    for i in range(n - 2):\n        if s[i:i+3] == \"ABC\":\n            count += 1\n    \n    return count\n\ndef process_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    \n    results = []\n    count = count_abc_substrings(s)\n    \n    index = 3\n    for _ in range(q):\n        x = int(data[index]) - 1\n        c = data[index + 1]\n        \n        # Update the character at position x\n        old_char = s[x]\n        s = s[:x] + c + s[x+1:]\n        \n        # Adjust the count of \"ABC\" substrings\n        if old_char == 'A':\n            if x > 0 and s[x-1] == 'B' and x < n-1 and s[x+1] == 'C':\n                count -= 1\n        elif old_char == 'B':\n            if x > 0 and s[x-1] == 'A' and x < n-1 and s[x+1] == 'C':\n                count += 1\n                if x > 1 and s[x-2] == 'A' and s[x-1] == 'B':\n                    count -= 1\n        elif old_char == 'C':\n            if x > 0 and s[x-1] == 'A' and x < n-1 and s[x+1] == 'B':\n                count -= 1\n        \n        if new_char == 'A':\n            if x > 0 and s[x-1] == 'B' and x < n-1 and s[x+1] == 'C':\n                count += 1\n        elif new_char == 'B':\n            if x > 0 and s[x-1] == 'A' and x < n-1 and s[x+1] == 'C':\n                count += 1\n                if x > 1 and s[x-2] == 'A' and s[x-1] == 'B':\n                    count += 1\n        elif new_char == 'C':\n            if x > 0 and s[x-1] == 'A' and x < n-1 and s[x+1] == 'B':\n                count += 1\n        \n        results.append(count)\n        index += 2\n    \n    for result in results:\n        print(result)\n\nprocess_queries()", "```python\ndef count_abc_substrings(n, q, s, queries):\n    def count_abc(s):\n        count = 0\n        n = len(s)\n        for i in range(2, n):\n            if s[i] == 'C':\n                count += int(s[i-2] == 'A' and s[i-1] == 'B')\n        return count\n\n    abc_count = count_abc(s)\n    a_counts = [0] * (n + 1)\n    b_counts = [0] * (n + 1)\n\n    for i in range(1, n):\n        a_counts[i] = a_counts[i - 1]\n        if s[i - 1] == 'A':\n            a_counts[i] += 1\n\n        b_counts[i] = b_counts[i - 1]\n        if s[i - 1] == 'B':\n            b_counts[i] += 1\n\n    results = []\n    for x, c in queries:\n        x -= 1\n        old_char = s[x]\n        s = s[:x] + c + s[x+1:]\n\n        if old_char == 'A':\n            a_counts[x] -= 1\n        elif old_char == 'B':\n            b_counts[x] -= 1\n\n        if c == 'A':\n            a_counts[x] += 1\n        elif c == 'B':\n            b_counts[x] += 1\n\n        new_abc_count = 0\n        for i in range(x, n - 1):\n            if s[i] == 'C':\n                new_abc_count += int(a_counts[i] > 0 and b_counts[i] > 0)\n\n        results.append(new_abc_count)\n\n    for result in results:\n        print(result)\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nn = int(data[0])\nq = int(data[1])\ns = data[2]\nqueries = [(int(data[3 + 2 * i]) - 1, data[4 + 2 * i]) for i in range(q)]\n\ncount_abc_substrings(n, q, s, queries)", "```python\ndef count_abc(s):\n    count = 0\n    n = len(s)\n    for i in range(n - 2):\n        if s[i:i+3] == \"ABC\":\n            count += 1\n    return count\n\ndef process_queries(n, q, s, queries):\n    result = []\n    abc_count = count_abc(s)\n    result.append(abc_count)\n    \n    for x, c in queries:\n        x -= 1  # Convert to 0-based index\n        if s[x] == c:\n            continue\n        \n        # Remove the old character from the count\n        if x >= 2 and s[x-2:x+1] == \"ABC\":\n            abc_count -= 1\n        \n        # Add the new character to the count\n        if x <= n-3 and s[x:x+3] == \"ABC\":\n            abc_count += 1\n        \n        s = s[:x] + c + s[x+1:]\n        result.append(abc_count)\n    \n    return result\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[3 + 2 * i]) for i in range(q)]\n    \n    result = process_queries(n, q, s, queries)\n    for count in result:\n        print(count)\n\nif __name__ == \"__main__\":\n    main()\n```", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(Q)]\n\n    def count_ABC(s):\n        count = 0\n        n = len(s)\n        for i in range(n - 2):\n            if s[i:i+3] == 'ABC':\n                count += 1\n        return count\n\n    current_count = count_ABC(S)\n    print(current_count)\n\n    for X, C in queries:\n        original_char = S[X - 1]\n        S = S[:X - 1] + C + S[X :]\n        if original_char == 'A' and C == 'B' and X <= len(S) - 2 and S[X:X+2] == 'BC':\n            current_count += 1\n        elif original_char == 'B' and C == 'C' and X <= len(S) - 2 and S[X:X+2] == 'CA':\n            current_count += 1\n        elif original_char == 'A' and C != 'B' and X > 1 and S[X-2:X] == 'AB':\n            current_count -= 1\n        elif original_char == 'B' and C != 'C' and X > 1 and S[X-2:X] == 'BC':\n            current_count -= 1\n        print(current_count)\n\nmain()", "```python\ndef count_abc_substrings(s):\n    count = 0\n    n = len(s)\n    for i in range(n - 2):\n        if s[i:i+3] == \"ABC\":\n            count += 1\n    return count\n\ndef process_queries(N, Q, S, queries):\n    result = []\n    abc_count = count_abc_substrings(S)\n    result.append(abc_count)\n    \n    for X, C in queries:\n        X -= 1  # Convert to 0-based index\n        if S[X] == C:\n            result.append(abc_count)\n            continue\n        \n        # Update the string\n        S = S[:X] + C + S[X+1:]\n        \n        # Adjust the count of \"ABC\" substrings\n        if X > 0 and S[X-1] == 'A' and S[X] == 'B' and S[X+1] == 'C':\n            abc_count += 1\n        if X > 1 and S[X-2] == 'A' and S[X-1] == 'B' and S[X] == 'C':\n            abc_count -= 1\n        if X < N - 1 and S[X] == 'A' and S[X+1] == 'B' and S[X+2] == 'C':\n            abc_count += 1\n        if X < N - 2 and S[X+1] == 'A' and S[X+2] == 'B' and S[X+3] == 'C':\n            abc_count -= 1\n        \n        result.append(abc_count)\n    \n    for r in result:\n        print(r)\n\n# Read input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nQ = int(data[1])\nS = data[2]\nqueries = [(int(data[2 + 2 * i]) - 1, data[3 + 2 * i]) for i in range(Q)]\n\n# Process queries and print results\nprocess_queries(N, Q, S, queries)", "```python\ndef process_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[2 + 2 * i + 1]) for i in range(Q)]\n    \n    # Initialize prefix sums\n    prefix_A = [0] * (N + 1)\n    prefix_B = [0] * (N + 1)\n    prefix_C = [0] * (N + 1)\n    \n    # Calculate initial prefix sums\n    for i in range(N):\n        if S[i] == 'A':\n            prefix_A[i + 1] = prefix_A[i] + 1\n        elif S[i] == 'B':\n            prefix_B[i + 1] = prefix_B[i] + 1\n        elif S[i] == 'C':\n            prefix_C[i + 1] = prefix_C[i] + 1\n    \n    def get_count(left, right):\n        return prefix_A[right] - prefix_A[left] + prefix_B[right] - prefix_B[left] + prefix_C[right] - prefix_C[left]\n    \n    result = []\n    current_count = get_count(0, N)\n    result.append(current_count)\n    \n    for x, c in queries:\n        old_char = S[x]\n        S = S[:x] + c + S[x+1:]\n        \n        if old_char == 'A':\n            prefix_A[x + 1] -= 1\n        elif old_char == 'B':\n            prefix_B[x + 1] -= 1\n        elif old_char == 'C':\n            prefix_C[x + 1] -= 1\n        \n        if c == 'A':\n            prefix_A[x + 1] += 1\n        elif c == 'B':\n            prefix_B[x + 1] += 1\n        elif c == 'C':\n            prefix_C[x + 1] += 1\n        \n        left = max(0, x - 1)\n        right = min(N, x + 2)\n        current_count = get_count(left, right)\n        result.append(current_count)\n    \n    for r in result:\n        print(r)\n\nprocess_queries()", "def count_abc_substrings(s):\n    n = len(s)\n    prefix_sums = [0] * (n + 1)\n    \n    # Initialize prefix sums\n    for i in range(2, n):\n        prefix_sums[i + 1] = prefix_sums[i]\n        if s[i-2:i+1] == \"ABC\":\n            prefix_sums[i + 1] += 1\n    \n    def update(i, new_char):\n        nonlocal prefix_sums\n        prefix_sum = 0\n        for j in range(i, n):\n            if j >= 2:\n                prefix_sum += prefix_sums[j - 1]\n            if s[j-2:j+1] == \"ABC\":\n                prefix_sum += 1\n            prefix_sums[j] = prefix_sum\n    \n    def get_count(i):\n        return prefix_sums[i]\n    \n    return update, get_count\n\ndef process_queries(n, q, s, queries):\n    update, get_count = count_abc_substrings(s)\n    results = []\n    for x, c in queries:\n        x -= 1  # Convert to 0-based index\n        update(x, c)\n        results.append(get_count(n))\n    return results\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(q)]\n    \n    results = process_queries(n, q, s, queries)", "```python\ndef process_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(Q)]\n    \n    def count_ABC(s):\n        count = 0\n        for i in range(len(s) - 2):\n            if s[i:i+3] == 'ABC':\n                count += 1\n        return count\n    \n    current_count = count_ABC(S)\n    print(current_count)\n    \n    for X, C in queries:\n        old_char = S[X - 1]\n        S = S[:X - 1] + C + S[X:]\n        \n        # Update the count of \"ABC\" substrings using sliding window\n        if X > 1 and S[X - 2:X] == 'AB' and C == 'C':\n            current_count += 1\n        elif X > 1 and S[X - 2:X] == 'A' and C == 'B':\n            current_count -= 1\n        \n        if X < len(S) and S[X - 1:X + 1] == 'BC' and old_char == 'A':\n            current_count -= 1\n        elif X < len(S) and S[X - 1:X + 1] == 'AB' and old_char == 'C':\n            current_count += 1\n        \n        print(current_count)\n\nprocess_queries()", "```python\ndef process_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[2 + 2 * i + 1]) for i in range(Q)]\n    \n    count = 0\n    for i in range(2, N):\n        if S[i-2:i+1] == \"ABC\":\n            count += 1\n    \n    print(count)\n    \n    for x, c in queries:\n        if S[x] == \"A\" and S[x+1] == \"B\" and S[x+2] == \"C\":\n            count -= 1\n        S = S[:x] + c + S[x+1:]\n        if x > 0 and x + 2 < N:\n            if S[x-1:x+3] == \"ABC\":\n                count += 1\n        if x > 1 and x + 1 < N:\n            if S[x-2:x+2] == \"ABC\":\n                count += 1\n        if x > 0 and x + 1 < N:\n            if S[x-1:x+3] == \"ABC\":\n                count -= 1\n        print(count)\n\nprocess_queries()", "```python\ndef count_abc_after_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    \n    # Initial count of \"ABC\"\n    count = 0\n    for i in range(N - 2):\n        if S[i:i+3] == \"ABC\":\n            count += 1\n    \n    results = []\n    index = 3\n    for _ in range(Q):\n        X = int(data[index]) - 1\n        C = data[index + 1]\n        \n        # Update the sliding window\n        if X > 0:\n            if S[X-1:X+2] == \"ABC\":\n                count -= 1\n        if X < N - 2:\n            if S[X+1:X+4] == \"ABC\":\n                count -= 1\n        \n        S = S[:X] + C + S[X+1:]\n        \n        if X > 0:\n            if S[X-1:X+2] == \"ABC\":\n                count += 1\n        if X < N - 2:\n            if S[X+1:X+4] == \"ABC\":\n                count += 1\n        \n        results.append(count)\n        index += 2\n    \n    for result in results:\n        print(result)\n\ncount_abc_after_queries()"]}
