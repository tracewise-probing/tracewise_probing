G and H isomorphic\n    min_cost = float('inf')\n    for i in range(N):\n        min_cost = min(min_cost, dp[i][(1 << N) - 1])\n\n    print(min_cost)\n\nmin_cost_to_isomorphic()", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphize_graphs(N, M_G, edges_G, M_H, edges_H, costs):\n    # Create adjacency lists for both graphs\n    adj_G = defaultdict(list)\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n\n    adj_H = defaultdict(list)\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n\n    # Calculate the cost matrix for transforming H into G\n    cost_matrix = [[0] * (N + 1) for _ in range(N + 1)]\n    for i in range(1, N + 1):\n        for j in range(i + 1, N + 1):\n            if (i, j) in costs:\n                cost_matrix[i][j] = costs[(i, j)]\n                cost_matrix[j][i] = costs[(i, j)]\n\n    # Initialize DP table\n    dp = [[float('inf')] * (1 << N) for _ in range(N + 1)]\n    dp[0][0] = 0\n\n    # Fill DP table\n    for i in range(1, N + 1):\n        for state in range(1 << N):\n            if (state & (1 << (i - 1))) == 0:\n                continue\n\n            for j in range(1, N + 1):\n                if (state & (1 << (j - 1))) == 0:\n                    continue\n\n                if i != j:\n                    new_state = state ^ (1 << (i - 1)) ^ (1 << (j - 1))\n                    dp[i][new_state] = min(dp[i][new_state], dp[j][state] + cost_matrix[i][j])\n\n    # Find the minimum cost to make H isomorphic to G\n    min_cost = float('inf')\n    for state in range(1 << N):\n        if all(len(adj_G[i]) == len(adj_H[i]) for i in range(1, N + 1)):\n            min_cost = min(min_cost, max(dp[i][state] for i in range(1, N + 1)))\n\n    return min_cost\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index])\n        index += 1\n        v = int(data[index])\n        index += 1\n        edges_G.append((u, v))\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index])\n        index += 1\n        b = int(data[index])\n        index += 1\n        edges_H.append((a, b))\n\n    costs = {}\n    for i in range(1, N + 1):\n        for j in range(i + 1, N + 1):\n            costs[(i, j)] = int(data[index])\n            index += 1\n\n    result = min_cost_to_isomorphize_graphs(N, M_G, edges_G, M_H, edges_H, costs)\n    print(result)", "```python\nfrom itertools import permutations\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs):\n    # Convert edge lists to adjacency matrices\n    adj_G = [[0] * N for _ in range(N)]\n    for u, v in edges_G:\n        adj_G[u-1][v-1] = 1\n        adj_G[v-1][u-1] = 1\n    \n    adj_H = [[0] * N for _ in range(N)]\n    for a, b in edges_H:\n        adj_H[a-1][b-1] = 1\n        adj_H[b-1][a-1] = 1\n    \n    min_cost = float('inf')\n    \n    # Generate all permutations of vertices for H\n    for perm in permutations(range(N)):\n        perm_adj_H = [[adj_H[perm[i]][perm[j]] for j in range(N)] for i in range(N)]\n        \n        # Calculate the cost to make perm_adj_H isomorphic to adj_G\n        cost = 0\n        for i in range(N):\n            for j in range(i+1, N):\n                if adj_G[i][j] != perm_adj_H[i][j]:\n                    cost += costs[i][j]\n        \n        # Update the minimum cost\n        min_cost = min(min_cost, cost)\n    \n    print(min_cost)\n\n# Read input from stdin\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM_G = int(data[1])\nedges_G = [(int(data[2 + 2 * i]) - 1, int(data[3 + 2 * i]) - 1) for i in range(M_G)]\nM_H = int(data[2 + 2 * M_G])\nedges_H = [(int(data[3 + 2 * M_G + 2 * i]) - 1, int(data[4 + 2 * M_G + 2 * i]) - 1) for i in range(M_H)]\ncosts = []\nfor i in range(N - 1):\n    costs.append([int(data[5 + 2 * M_G + 2 * M_H + 2 * i + j]) for j in range(N - i - 1)])\n\n# Call the function with the input data\nmin_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, costs)", "```python\nimport sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorph():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    G = [[0] * N for _ in range(N)]\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        G[u][v] = 1\n        G[v][u] = 1\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    H = [[0] * N for _ in range(N)]\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        H[a][b] = 1\n        H[b][a] = 1\n        index += 2\n    \n    A = []\n    for i in range(N - 1):\n        A.append(list(map(int, data[index:index + N - i - 1])))\n        index += N - i - 1\n    \n    min_cost = float('inf')\n    \n    for perm in permutations(range(N)):\n        cost = 0\n        H_perm = [[H[perm[i]][perm[j]] for j in range(N)] for i in range(N)]\n        for i in range(N):\n            for j in range(i + 1, N):\n                if G[i][j] != H_perm[i][j]:\n                    cost += A[i][j]\n        min_cost = min(min_cost, cost)\n    \n    print(min_cost)\n\nmin_cost_to_isomorph()", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs):\n    # Create adjacency lists for both graphs\n    adj_g = defaultdict(list)\n    for u, v in edges_g:\n        adj_g[u].append(v)\n        adj_g[v].append(u)\n\n    adj_h = defaultdict(list)\n    for a, b in edges_h:\n        adj_h[a].append(b)\n        adj_h[b].append(a)\n\n    # Calculate the cost to make G and H isomorphic\n    total_cost = 0\n    for i in range(1, n + 1):\n        for j in range(i + 1, n + 1):\n            edge_exists_g = j in adj_g[i]\n            edge_exists_h = j in adj_h[i]\n\n            if edge_exists_g != edge_exists_h:\n                total_cost += costs[i - 1][j - 1]\n\n    return total_cost\n\n# Read input from standard input\ninput = sys.stdin.read\ndata = input().split()\n\nindex = 0\nn = int(data[index])\nindex += 1\nm_g = int(data[index])\nindex += 1\nedges_g = []\nfor _ in range(m_g):\n    u = int(data[index])\n    index += 1\n    v = int(data[index])\n    index += 1\n    edges_g.append((u, v))\nm_h = int(data[index])\nindex += 1\nedges_h = []\nfor _ in range(m_h):\n    a = int(data[index])\n    index += 1\n    b = int(data[index])\n    index += 1\n    edges_h.append((a, b))\ncosts = []\nfor _ in range(n - 1):\n    row = list(map(int, data[index:index + n - 1]))\n    index += n - 1\n    costs.append(row)\n\n# Calculate and print the result\nresult = min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs)\nprint(result)", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    def read_int():\n        nonlocal index\n        result = int(data[index])\n        index += 1\n        return result\n    \n    N = read_int()\n    M_G = read_int()\n    edges_G = [(read_int(), read_int()) for _ in range(M_G)]\n    M_H = read_int()\n    edges_H = [(read_int(), read_int()) for _ in range(M_H)]\n    A = [[read_int() for _ in range(N)] for _ in range(N)]\n    \n    # Convert edges to sets for easy lookup\n    adj_G = [set() for _ in range(N)]\n    for u, v in edges_G:\n        adj_G[u-1].add(v-1)\n        adj_G[v-1].add(u-1)\n    \n    adj_H = [set() for _ in range(N)]\n    for u, v in edges_H:\n        adj_H[u-1].add(v-1)\n        adj_H[v-1].add(u-1)\n    \n    # Precompute the cost to add/remove edges\n    cost_matrix = [[0] * N for _ in range(N)]\n    for i in range(N):\n        for j in range(i+1, N):\n            if (i, j) not in adj_H:\n                cost_matrix[i][j] = A[i][j]\n            else:\n                cost_matrix[i][j] = -A[i][j]\n    \n    # DP table\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    for mask in range(1 << N):\n        count = bin(mask).count('1')\n        if count == 0:\n            continue\n        \n        for i in range(N):\n            if mask & (1 << i):\n                new_mask = mask\n                for j in range(i+1, N):\n                    if mask & (1 << j):\n                        if (i, j) not in adj_H:\n                            new_mask |= (1 << j)\n                        else:\n                            new_mask ^= (1 << j)\n                \n                dp[new_mask] = min(dp[new_mask], dp[mask] + abs(cost_matrix[i][j]))\n    \n    print(min(dp))\n\nmin_cost_to_isomorphic()", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphize_graphs(N, M_G, edges_G, M_H, edges_H, costs):\n    # Create adjacency lists for both graphs\n    adj_G = defaultdict(list)\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n\n    adj_H = defaultdict(list)\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n\n    # Function to calculate the difference in adjacency matrices\n    def diff_matrix(adj_G, adj_H):\n        diff = 0\n        for i in range(1, N + 1):\n            for j in range(i + 1, N + 1):\n                if (j in adj_G[i]) != (j in adj_H[i]):\n                    diff += abs(costs[i - 1][j - 1])\n        return diff\n\n    # Calculate initial difference\n    initial_diff = diff_matrix(adj_G, adj_H)\n\n    # Try to minimize the difference by flipping edges\n    min_cost = float('inf')\n    for i in range(1, N + 1):\n        for j in range(i + 1, N + 1):\n            if j not in adj_H[i]:\n                new_adj_H = adj_H.copy()\n                new_adj_H[i].append(j)\n                new_adj_H[j].append(i)\n                new_diff = diff_matrix(adj_G, new_adj_H)\n                if new_diff < initial_diff:\n                    min_cost = min(min_cost, new_diff)\n            elif j in adj_H[i]:\n                new_adj_H = adj_H.copy()\n                new_adj_H[i].remove(j)\n                new_adj_H[j].remove(i)\n                new_diff = diff_matrix(adj_G, new_adj_H)\n                if new_diff < initial_diff:\n                    min_cost = min(min_cost, new_diff)\n\n    return min_cost\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index])\n        index += 1\n        v = int(data[index])\n        index += 1\n        edges_G.append((u, v))\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index])\n        index += 1\n        b = int(data[index])\n        index += 1\n        edges_H.append((a, b))\n\n    costs = []\n    for i in range(N - 1):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        costs.append(row)\n\n    result = min_cost_to_isomorphize_graphs(N, M_G, edges_G, M_H, edges_H, costs)\n    print(result)", "```python\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\n# Read input\nN = int(data[0])\nM_G = int(data[1])\nedges_G = [(int(data[2 + 2 * i]), int(data[3 + 2 * i])) for i in range(M_G)]\nM_H = int(data[2 + 2 * M_G])\nedges_H = [(int(data[4 + 2 * M_G + 2 * i]), int(data[5 + 2 * M_G + 2 * i])) for i in range(M_H)]\ncosts = [[int(data[6 + 2 * M_G + 2 * M_H + 3 * i + j]) for j in range(i + 1)] for i in range(N)]\n\n# Function to get the bitmask representation of edges in H\ndef get_mask(edges):\n    mask = 0\n    for u, v in edges:\n        mask |= (1 << ((u - 1) * N + v - 1))\n    return mask\n\n# Get initial masks\nmask_G = get_mask(edges_G)\nmask_H = get_mask(edges_H)\n\n# Initialize DP array\ndp = [float('inf')] * (1 << (N * N))\ndp[mask_H] = 0\n\n# Directions for iterating over pairs (i, j)\ndirections = [(i, j) for i in range(N) for j in range(i + 1, N)]\n\n# Fill DP array\nfor mask in range(1 << (N * N)):\n    for i, j in directions:\n        bit = (1 << ((i - 1) * N + j - 1))\n        if mask & bit:\n            dp[mask] = min(dp[mask], dp[mask ^ bit] + costs[i][j])\n        else:\n            dp[mask] = min(dp[mask], dp[mask ^ bit] + costs[i][j])\n\n# Find the minimum cost to make H isomorphic to G\nmin_cost = float('inf')\nfor i in range(N):\n    for j in range(i + 1, N):\n        bit = (1 << ((i - 1) * N + j - 1))\n        if mask_G & bit:\n            min_cost = min(min_cost, dp[mask_G ^ bit] + costs[i][j])\n        else:\n            min_cost = min(min_cost, dp[mask_G ^ bit] + costs[i][j])\n\nprint(min_cost)\n```", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphize_graphs():\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n\n    M_G = int(data[index])\n    index += 1\n    edges_G = [tuple(map(int, data[index:index+2])) for _ in range(M_G)]\n    index += 2 * M_G\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = [tuple(map(int, data[index:index+2])) for _ in range(M_H)]\n    index += 2 * M_H\n\n    A = [[int(data[index + i * N + j]) for j in range(N - 1)] for i in range(N - 1)]\n    index += N * (N - 1)\n\n    adj_G = defaultdict(set)\n    for u, v in edges_G:\n        adj_G[u].add(v)\n        adj_G[v].add(u)\n\n    adj_H = defaultdict(set)\n    for u, v in edges_H:\n        adj_H[u].add(v)\n        adj_H[v].add(u)\n\n    def get_adj_matrix(adj):\n        return [[1 if v in adj[u] else 0 for v in range(1, N + 1)] for u in range(1, N + 1)]\n\n    def hamming_distance(A, B):\n        return sum(sum(abs(a - b) for a, b in zip(row_A, row_B)) for row_A, row_B in zip(A, B))\n\n    adj_matrix_G = get_adj_matrix(adj_G)\n    adj_matrix_H = get_adj_matrix(adj_H)\n\n    min_cost = float('inf')\n    for perm in permutations(range(1, N + 1)):\n        permuted_adj_H = get_adj_matrix({perm[i]: perm[j] for i, j in adj_H.items()})\n        cost = hamming_distance(adj_matrix_G, permuted_adj_H)\n        min_cost = min(min_cost, cost)\n\n    print(min_cost)\n\nif __name__ == '__main__':\n    min_cost_to_isomorphize_graphs()", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    def next_int():\n        nonlocal index\n        result = int(data[index])\n        index += 1\n        return result\n    \n    N = next_int()\n    M_G = next_int()\n    G_edges = [(next_int(), next_int()) for _ in range(M_G)]\n    M_H = next_int()\n    H_edges = [(next_int(), next_int()) for _ in range(M_H)]\n    A = [[next_int() for _ in range(N)] for _ in range(N)]\n    \n    # Create adjacency matrices for G and H\n    adj_G = [[0] * N for _ in range(N)]\n    adj_H = [[0] * N for _ in range(N)]\n    for u, v in G_edges:\n        adj_G[u-1][v-1] = 1\n        adj_G[v-1][u-1] = 1\n    for a, b in H_edges:\n        adj_H[a-1][b-1] = 1\n        adj_H[b-1][a-1] = 1\n    \n    # Convert adjacency matrices to bitmasks\n    mask_G = sum(1 << ((i-1) * N + (j-1)) for i in range(1, N+1) for j in range(i+1, N+1) if adj_G[i-1][j-1])\n    mask_H = sum(1 << ((i-1) * N + (j-1)) for i in range(1, N+1) for j in range(i+1, N+1) if adj_H[i-1][j-1])\n    \n    # Initialize DP table\n    dp = [float('inf')] * (1 << (N * (N - 1) // 2))\n    dp[mask_H] = 0\n    \n    # Iterate over all possible masks\n    for mask in range((1 << (N * (N - 1) // 2)) - 1):\n        for i in range(N):\n            for j in range(i+1, N):\n                bit = 1 << ((i-1) * N + (j-1))\n                if mask & bit == 0:\n                    new_mask = mask | bit\n                    dp[new_mask] = min(dp[new_mask], dp[mask] + A[i][j])\n                else:\n                    new_mask = mask ^ bit\n                    dp[new_mask] = min(dp[new_mask], dp[mask] + A[i][j])\n    \n    # Find the minimum cost to reach the target mask\n    min_cost = float('inf')\n    for i in range(N):\n        for j in range(i+1, N):\n            bit = 1 << ((i-1) * N + (j-1))\n            if mask_G & bit != mask_H & bit:\n                min_cost = min(min_cost, dp[mask_H ^ bit] + A[i][j])\n    \n    print(min_cost)\n\nmin_cost_to_isomorphic()"]}
