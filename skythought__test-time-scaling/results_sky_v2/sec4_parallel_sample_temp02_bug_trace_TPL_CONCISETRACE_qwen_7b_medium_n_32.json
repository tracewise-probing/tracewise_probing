1\n\n    M_G = int(data[index])\n    index += 1\n    edges_G = [(int(data[index]), int(data[index + 1])) for _ in range(M_G)]\n    index += 2\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = [(int(data[index]), int(data[index + 1])) for _ in range(M_H)]\n    index += 2\n\n    A = [[int(data[index + j * N + i]) for i in range(N)] for j in range(N)]\n    index += N * N\n\n    def is_edge(graph, u, v):\n        return (u, v) in graph or (v, u) in graph\n\n    @lru_cache(None)\n    def dp(i, j, graph):\n        if i == N:\n            return 0\n\n        cost = float('inf')\n\n        if is_edge(graph, i, j):\n            if not is_edge(edges_H, i, j):\n                cost = min(cost, A[i][j] + dp(i + 1, j, graph))\n            else:\n                cost = min(cost, dp(i + 1, j, graph))\n        else:\n            if is_edge(edges_H, i, j):\n                cost = min(cost, A[i][j] + dp(i + 1, j, graph))\n            else:\n                cost = min(cost, dp(i + 1, j, graph))\n\n        return cost\n\n    min_cost = float('inf')\n\n    for perm in range(N):\n        graph = edges_H[:]\n        for k in range(N):\n            if k != perm:\n                graph.append((k + 1, perm + 1))\n        min_cost = min(min_cost, dp(0, perm, tuple(graph)))\n\n    print(min_cost)\n\nif __name__ == '__main__':\n    main()"]}
