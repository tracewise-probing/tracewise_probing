 += key - current_right\n                    current_right = key\n                elif current_right > key:\n                    fatigue += current_right - key\n                    current_right = key\n        \n        return fatigue\n    \n    min_fatigue = float('inf')\n    for i in range(1, 101):\n        for j in range(i, 101):\n            min_fatigue = min(min_fatigue, dp(i, j))\n    \n    print(min_fatigue)\n\nmin_fatigue_level()\n```", "def min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    N = int(data[0])\n    actions = [(int(data[i*2+1]), data[i*2+2]) for i in range(N)]\n    left_hand, right_hand = 0, 0\n    fatigue = 0\n    for action in actions:\n        key, hand_side = action\n        if hand_side == 'L':\n            if key < left_hand:\n                fatigue += left_hand - key\n                left_hand = key\n            elif key > left_hand:\n                fatigue += key - left_hand\n                left_hand = key\n        else:\n            if key < right_hand:\n                fatigue += right_hand - key\n                right_hand = key\n            elif key > right_hand:\n                fatigue += key - right_hand\n                right_hand = key\n    return fatigue\n\nif __name__ == '__main__':\n    print(min_fatigue_level())", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    A = [int(data[i]) for i in range(1, 2*N, 2)]\n    S = [data[i] for i in range(2, 2*N + 1, 2)]\n    \n    def calculate_fatigue(l_hand, r_hand):\n        fatigue = 0\n        for i in range(N):\n            if S[i] == 'L':\n                if l_hand < A[i]:\n                    fatigue += A[i] - l_hand\n                    l_hand = A[i]\n                elif r_hand < A[i]:\n                    fatigue += A[i] - r_hand\n                    r_hand = A[i]\n            else:\n                if r_hand < A[i]:\n                    fatigue += A[i] - r_hand\n                    r_hand = A[i]\n                elif l_hand < A[i]:\n                    fatigue += A[i] - l_hand\n                    l_hand = A[i]\n        return fatigue\n    \n    min_fatigue = float('inf')\n    for l_init in range(1, 101):\n        for r_init in range(1, 101):\n            fatigue = calculate_fatigue(l_init, r_init)\n            if fatigue < min_fatigue:\n                min_fatigue = fatigue\n                \n    print(min_fatigue)\n\nmin_fatigue_level()\n```", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    presses = []\n    \n    for i in range(1, 2 * N, 2):\n        key = int(data[i])\n        hand = data[i + 1]\n        presses.append((key, hand))\n    \n    # Sort presses based on key positions\n    presses.sort(key=lambda x: x[0])\n    \n    # Initialize left and right hands\n    left_hand, right_hand = presses[0][0], presses[-1][0]\n    fatigue = 0\n    \n    # Process each press\n    for key, hand in presses:\n        if hand == 'L':\n            if left_hand < key:\n                fatigue += key - left_hand\n                left_hand = key\n        else:\n            if right_hand > key:\n                fatigue += right_hand - key\n                right_hand = key\n    \n    print(fatigue)\n\nmin_fatigue_level()", "```python\ndef min_fatigue():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    hands = {'left': None, 'right': None}\n    fatigue = 0\n    \n    for i in range(1, 2 * N, 2):\n        key = int(data[i])\n        hand = data[i + 1]\n        \n        if hands[hand] is None:\n            hands[hand] = key\n        else:\n            fatigue += abs(key - hands[hand])\n            hands[hand] = key\n    \n    print(fatigue)\n\nmin_fatigue()\n```", "```python\ndef min_fatigue():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    A = [int(data[2*i+1]) for i in range(N)]\n    S = [data[2*i+2] for i in range(N)]\n    \n    left_hand = [float('inf')] * 101\n    right_hand = [float('inf')] * 101\n    \n    left_hand[0] = 0\n    right_hand[0] = 0\n    \n    for i in range(N):\n        if S[i] == 'L':\n            left_hand[A[i]] = min(left_hand[A[i]], left_hand[0] + abs(A[i] - 0))\n            left_hand[0] = float('inf')\n        else:\n            right_hand[A[i]] = min(right_hand[A[i]], right_hand[0] + abs(A[i] - 0))\n            right_hand[0] = float('inf')\n        \n        for j in range(1, 101):\n            if S[i] == 'L':\n                left_hand[j] = min(left_hand[j], left_hand[j-1] + abs(j - (j-1)))\n                left_hand[j-1] = float('inf')\n            else:\n                right_hand[j] = min(right_hand[j], right_hand[j-1] + abs(j - (j-1)))\n                right_hand[j-1] = float('inf')\n    \n    print(min(left_hand[100], right_hand[100]))\n\nmin_fatigue()", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    A = []\n    S = []\n    for i in range(N):\n        A.append(int(data[2*i + 1]))\n        S.append(data[2*i + 2])\n    \n    left_hand = 0\n    right_hand = 0\n    fatigue = 0\n    \n    for i in range(N):\n        if S[i] == 'L':\n            if abs(A[i] - left_hand) < abs(A[i] - right_hand):\n                fatigue += abs(A[i] - left_hand)\n                left_hand = A[i]\n            else:\n                fatigue += abs(A[i] - right_hand)\n                right_hand = A[i]\n        else:\n            if abs(A[i] - right_hand) < abs(A[i] - left_hand):\n                fatigue += abs(A[i] - right_hand)\n                right_hand = A[i]\n            else:\n                fatigue += abs(A[i] - left_hand)\n                left_hand = A[i]\n    \n    print(fatigue)\n\nmin_fatigue_level()", "```python\ndef min_fatigue():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    left_hand = int(data[1])\n    right_hand = int(data[2])\n    fatigue = 0\n    \n    index = 3\n    for _ in range(N):\n        A_i = int(data[index])\n        S_i = data[index + 1]\n        index += 2\n        \n        if S_i == 'L':\n            if A_i < left_hand:\n                fatigue += left_hand - A_i\n                left_hand = A_i\n            elif A_i > right_hand:\n                fatigue += A_i - right_hand\n                right_hand = A_i\n        else:\n            if A_i < right_hand:\n                fatigue += right_hand - A_i\n                right_hand = A_i\n            elif A_i > left_hand:\n                fatigue += A_i - left_hand\n                left_hand = A_i\n                \n    print(fatigue)\n\nmin_fatigue()", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    A = [int(data[i*2 + 1]) for i in range(N)]\n    S = [data[i*2 + 2] for i in range(N)]\n    \n    min_fatigue = float('inf')\n    \n    # Try all possible initial positions for the left and right hands\n    for left_start in range(1, 101):\n        for right_start in range(1, 101):\n            left_hand = left_start\n            right_hand = right_start\n            fatigue = 0\n            \n            for i in range(N):\n                if S[i] == 'L':\n                    if A[i] != left_hand:\n                        fatigue += abs(A[i] - left_hand)\n                        left_hand = A[i]\n                else:\n                    if A[i] != right_hand:\n                        fatigue += abs(A[i] - right_hand)\n                        right_hand = A[i]\n            \n            min_fatigue = min(min_fatigue, fatigue)\n    \n    print(min_fatigue)\n\nmin_fatigue_level()\n```", "```python\ndef min_fatigue():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    A = []\n    S = []\n    for i in range(1, 2*N, 2):\n        A.append(int(data[i]))\n        S.append(data[i+1])\n    \n    # Initialize DP table\n    dp = {}\n    \n    def dp_key(left, right):\n        return f\"{left},{right}\"\n    \n    # Initial state: both hands on key 1\n    dp[dp_key(1, 1)] = 0\n    \n    for i in range(N):\n        a = A[i]\n        s = S[i]\n        new_dp = {}\n        \n        for key, fatigue in dp.items():\n            left, right = map(int, key.split(','))\n            \n            if s == 'L':\n                # Move left hand to key a\n                if dp_key(a, right) not in new_dp:\n                    new_dp[dp_key(a, right)] = fatigue + abs(a - left)\n                elif fatigue + abs(a - left) < new_dp[dp_key(a, right)]:\n                    new_dp[dp_key(a, right)] = fatigue + abs(a - left)\n                \n                # Keep left hand on key left\n                if dp_key(left, right) not in new_dp:\n                    new_dp[dp_key(left, right)] = fatigue\n                elif fatigue < new_dp[dp_key(left, right)]:\n                    new_dp[dp_key(left, right)] = fatigue\n                \n                # Move right hand to key a (not allowed since it's an L press)\n                pass\n            \n            elif s == 'R':\n                # Move right hand to key a\n                if dp_key(left, a) not in new_dp:\n                    new_dp[dp_key(left, a)] = fatigue + abs(a - right)\n                elif fatigue + abs(a - right) < new_dp[dp_key(left, a)]:\n                    new_dp[dp_key(left, a)] = fatigue + abs(a - right)\n                \n                # Keep right hand on key right\n                if dp_key(left, right) not in new_dp:\n                    new_dp[dp_key(left, right)] = fatigue\n                elif fatigue < new_dp[dp_key(left, right)]:\n                    new_dp[dp_key(left, right)] = fatigue\n                \n                # Move left hand to key a (not allowed since it's an R press)\n                pass\n        \n        dp = new_dp\n    \n    # Find the minimum fatigue level among all possible final states\n    min_fatigue = float('inf')\n    for key, fatigue in dp.items():\n        left, right = map(int, key.split(','))\n        min_fatigue = min(min_fatigue, fatigue)\n    \n    print(min_fatigue)\n\nmin_fatigue()", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    actions = [(int(data[i*2 + 1]), data[i*2 + 2]) for i in range(N)]\n    \n    left_hand = 1\n    right_hand = 1\n    fatigue = 0\n    \n    for action in actions:\n        key, hand = action\n        if hand == 'L':\n            if key < left_hand:\n                fatigue += abs(key - left_hand)\n                left_hand = key\n            elif key > left_hand:\n                fatigue += abs(key - left_hand)\n                left_hand = key\n        elif hand == 'R':\n            if key < right_hand:\n                fatigue += abs(key - right_hand)\n                right_hand = key\n            elif key > right_hand:\n                fatigue += abs(key - right_hand)\n                right_hand = key\n    \n    print(fatigue)\n\nmin_fatigue_level()\n```", "```python\ndef min_fatigue_level():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    left_hand = 1\n    right_hand = 1\n    fatigue = 0\n    \n    for i in range(1, 2 * N, 2):\n        key = int(data[i])\n        hand_side = data[i + 1]\n        \n        if hand_side == 'L':\n            if key < left_hand:\n                fatigue += left_hand - key\n                left_hand = key\n            elif key > left_hand:\n                left_hand = key\n        else:\n            if key < right_hand:\n                fatigue += right_hand - key\n                right_hand = key\n            elif key > right_hand:\n                right_hand = key\n    \n    print(fatigue)\n\nmin_fatigue_level()\n```", "```python\nimport sys\n\ndef min_fatigue(N, key_presses):\n    left_hand = 0\n    right_hand = 0\n    total_fatigue = 0\n    \n    for A_i, S_i in key_presses:\n        if S_i == 'L':\n            if right_hand == A_i:\n                continue\n            if left_hand == A_i:\n                total_fatigue += abs(right_hand - left_hand)\n                right_hand = left_hand\n            left_hand = A_i\n        else:  # S_i == 'R'\n            if left_hand == A_i:\n                continue\n            if right_hand == A_i:\n                total_fatigue += abs(right_hand - left_hand)\n                left_hand = right_hand\n            right_hand = A_i\n    \n    return total_fatigue\n\n# Read input from stdin\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nkey_presses = [(int(data[2*i+1]), data[2*i+2]) for i in range(N)]\n\n# Calculate and print the minimum fatigue level\nprint(min_fatigue(N, key_presses))\n```", "```python\ndef min_fatigue():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    presses = [(int(data[2*i + 1]), data[2*i + 2]) for i in range(N)]\n    \n    def calculate_fatigue(left_hand, right_hand):\n        fatigue = 0\n        current_left = left_hand\n        current_right = right_hand\n        \n        for key, hand in presses:\n            if hand == 'L':\n                if current_left != key:\n                    fatigue += abs(key - current_left)\n                    current_left = key\n            elif hand == 'R':\n                if current_right != key:\n                    fatigue += abs(key - current_right)\n                    current_right = key\n        return fatigue\n    \n    min_fatigue_level = float('inf')\n    \n    for left_hand in range(1, 101):\n        for right_hand in range(1, 101):\n            fatigue = calculate_fatigue(left_hand, right_hand)\n            if fatigue < min_fatigue_level:\n 