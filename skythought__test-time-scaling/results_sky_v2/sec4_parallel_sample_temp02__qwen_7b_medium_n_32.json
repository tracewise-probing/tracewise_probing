          cost += A[i][j]\n        return cost\n\n    # Try all permutations of vertices in H and find the minimum cost\n    min_cost = float('inf')\n    for perm in permutations(range(1, N+1)):\n        min_cost = min(min_cost, calculate_cost(perm))\n\n    return min_cost\n\n# Read input\nN, M_G = map(int, input().split())\nedges_G = [tuple(map(int, input().split())) for _ in range(M_G)]\nM_H = int(input())\nedges_H = [tuple(map(int, input().split())) for _ in range(M_H)]\nA = [list(map(int, input().split())) for _ in range(N-1)]\n\n# Calculate and print the minimum cost\nprint(min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A))", "```python\nimport sys\nfrom functools import lru_cache\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    def next_int():\n        nonlocal index\n        result = int(data[index])\n        index += 1\n        return result\n    \n    N = next_int()\n    M_G = next_int()\n    G_edges = [(next_int(), next_int()) for _ in range(M_G)]\n    M_H = next_int()\n    H_edges = [(next_int(), next_int()) for _ in range(M_H)]\n    A = [[next_int() for _ in range(N)] for _ in range(N)]\n    \n    return N, M_G, G_edges, M_H, H_edges, A\n\n@lru_cache(None)\ndef min_cost(i, j):\n    if i == N:\n        return 0\n    \n    min_cost_toggle = float('inf')\n    for k in range(N):\n        if (j + 1, k + 1) not in H_edges and (k + 1, j + 1) not in H_edges:\n            min_cost_toggle = min(min_cost_toggle, A[j][k] + min_cost(i + 1, k))\n        elif (j + 1, k + 1) in H_edges and (k + 1, j + 1) in H_edges:\n            min_cost_toggle = min(min_cost_toggle, min_cost(i + 1, k))\n    \n    return min_cost_toggle\n\nN, M_G, G_edges, M_H, H_edges, A = read_input()\n\n# Ensure H_edges contains both directions\nfor u, v in H_edges:\n    if (v, u) not in H_edges:\n        H_edges.append((v, u))\n\n# Find the minimum cost to make H isomorphic to G\nmin_total_cost = float('inf')\nfor perm in permutations(rang