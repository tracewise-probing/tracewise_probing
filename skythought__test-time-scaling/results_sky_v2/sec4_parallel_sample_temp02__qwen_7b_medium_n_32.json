= int(data[index])\n        index += 1\n        return result\n    \n    N = next_int()\n    M_G = next_int()\n    G_edges = [(next_int(), next_int()) for _ in range(M_G)]\n    M_H = next_int()\n    H_edges = [(next_int(), next_int()) for _ in range(M_H)]\n    A = [[next_int() for _ in range(N)] for _ in range(N)]\n    \n    return N, M_G, G_edges, M_H, H_edges, A\n\n@lru_cache(None)\ndef min_cost(i, H_state):\n    if i == N + 1:\n        return 0\n    \n    min_cost = float('inf')\n    for j in range(1, N + 1):\n        if (i, j) not in H_state:\n            new_state = H_state | {(i, j)}\n            cost = A[i - 1][j - 1]\n        else:\n            new_state = H_state - {(i, j)}\n            cost = A[i - 1][j - 1]\n        \n        min_cost = min(min_cost, cost + min_cost(j + 1, new_state))\n    \n    return min_cost\n\ndef main():\n    N, M_G, G_edges, M_H, H_edges, A = read_input()\n    \n    # Convert edge lists to sets for efficient lookup\n    G_set = set(G_edges)\n    H_set = set(H_edges)\n    \n    # Initialize DP state\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    # Iterate over all possible states of H\n    for state in range(1 << N):\n        for i in range(1, N + 1):\n            for j in range(i + 1, N + 1):\n                if (i, j) not in H_set:\n                    new_state = state | (1 << (i - 1)) | (1 << (j - 1))\n                    cost = A[i - 1][j - 1]\n                else:\n                    new_state = state & ~(1 << (i - 1)) & ~(1 << (j - 1))\n                    cost = A[i - 1][j - 1]\n                \n                if (new_state >> (i - 1)) & 1 and (new_state >> (j - 1)) & 1:\n                    dp[new_state] = min(dp[new_state], dp[state] + cost)\n    \n    print(dp[(1 << N) - 1])\n\nif __name__ == \"__main__\":\n    main()\n```", "import sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorphize(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n\n    # Generate all permutations of vertex map