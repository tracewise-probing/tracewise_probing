      \n        # Adjust the count based on the change\n        if old_char == 'A' and c == 'B':\n            if x < n - 2 and s[x:x+3] == 'ABC':\n                current_count += 1\n        elif old_char == 'B' and c == 'C':\n            if x < n - 2 and s[x:x+3] == 'ABC':\n                current_count += 1\n        elif old_char == 'A' and c != 'B':\n            if x < n - 2 and s[x+1:x+3] == 'BC':\n                current_count -= 1\n        elif old_char == 'B' and c != 'C':\n            if x > 0 and x < n - 1 and s[x-1:x+2] == 'AB':\n                current_count -= 1\n        \n        result.append(current_count)\n    \n    for r in result:\n        print(r)\n\nprocess_queries()", "```python\ndef count_abc(s):\n    count = 0\n    n = len(s)\n    for i in range(n - 2):\n        if s[i:i+3] == \"ABC\":\n            count += 1\n    return count\n\ndef process_queries(n, q, s, queries):\n    result = []\n    current_count = count_abc(s)\n    result.append(current_count)\n    \n    for x, c in queries:\n        x -= 1  # Convert to 0-based index\n        if s[x] == c:\n            continue\n        \n        # Adjust the count of \"ABC\" based on the characters sliding out and in\n        if x > 0 and s[x-1:x+2] == \"ABC\":\n            current_count -= 1\n        if x < n - 2 and s[x:x+3] == \"ABC\":\n            current_count += 1\n        \n        s = s[:x] + c + s[x+1:]\n        result.append(current_count)\n    \n    return result\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[2 + 2 * i + 1]) for i in range(q)]\n    \n    result = process_queries(n, q, s, queries)\n    for r in result:\n        print(r)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef count_abc(s):\n    n = len(s)\n    count = 0\n    for i in range(n - 2):\n        if s[i:i+3] == 'ABC':\n            count += 1\n    return count\n\ndef update_and_count(s, x, c):\n    n = len(s)\n    old_char = s[x-1]\n    new_char = c\n    \n    # Adjust counts based on the change\n    if old_char == 'A' and new_char == 'B':\n        count = 1\n    elif old_char == 'B' and new_char == 'C':\n        count = 1\n    elif old_char == 'A' and new_char == 'C':\n        count = 1\n    else:\n        count = 0\n    \n    return count\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    \n    initial_count = count_abc(s)\n    print(initial_count)\n    \n    index = 3\n    for _ in range(q):\n        x = int(data[index]) - 1\n        c = data[index + 1]\n        index += 2\n        \n        count = update_and_count(s, x, c)\n        initial_count += count\n        print(initial_count)\n\nif __name__ == \"__main__\":\n    main()\n```", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(Q)]\n\n    def count_ABC(s):\n        return s.count('ABC')\n\n    def update_and_count(S, X, C):\n        nonlocal count\n        old_char = S[X - 1]\n        S = S[:X - 1] + C + S[X :]\n        if old_char == 'A':\n            count -= (old_char + S[X - 2:X]).count('AB')\n        elif old_char == 'B':\n            count -= (S[X - 2:X] + old_char).count('BA')\n        elif old_char == 'C':\n            count -= (S[X - 3:X - 1] + old_char).count('CA')\n        if C == 'A':\n            count += (S[X - 2:X]).count('AB')\n        elif C == 'B':\n            count += (S[X - 2:X] + C).count('BA')\n        elif C == 'C':\n            count += (S[X - 3:X - 1] + C).count('CA')\n        return S, count\n\n    count = count_ABC(S)\n    results = []\n    for X, C in queries:\n        S, count = update_and_count(S, X, C)\n        results.append(count)\n\n    for result in results:\n        print(result)\n\nmain()"]}
