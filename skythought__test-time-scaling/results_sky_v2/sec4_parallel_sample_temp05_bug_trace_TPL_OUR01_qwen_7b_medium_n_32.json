    for u, v in edges_G:\n        G_adj[u][v] = 1\n        G_adj[v][u] = 1\n    \n    min_cost = float('inf')\n    for perm in permutations(range(N)):\n        H_adj = [[0] * N for _ in range(N)]\n        for a, b in edges_H:\n            H_adj[a][b] = 1\n            H_adj[b][a] = 1\n        \n        for i in range(N - 1):\n            for j in range(i + 1, N):\n                H_adj[perm[i]][perm[j]] = A[i][j]\n        \n        if is_isomorphic(G_adj, H_adj):\n            cost = sum(A[i][j] for i in range(N - 1) for j in range(i + 1, N) if H_adj[perm[i]][perm[j]])\n            min_cost = min(min_cost, cost)\n    \n    return min_cost if min_cost != float('inf') else 0\n\ndef main():\n    N, M_G, edges_G, M_H, edges_H, A = parse_input()\n    result = min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\nimport sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(N):\n        row = []\n        for j in range(i + 1, N):\n            cost = int(data[index])\n            index += 1\n            row.append(cost)\n        A.append(row)\n    \n    # Build adjacency lists for G and H\n    adj_G = defaultdict(list)\n    for u, v in edges_G:\n        adj_G[u].append(v)\n        adj_G[v].append(u)\n    \n    adj_H = defaultdict(list)\n    for a, b in edges_H:\n        adj_H[a].append(b)\n        adj_H[b].append(a)\n    \n    # Initialize DP table\n    dp = [[float('inf')] * (1 << N) for _ in range(1 << N)]\n    \n    def dfs(mask_G, mask_H):\n        if dp[mask_G][mask_H] != float('inf'):\n            return dp[mask_G][mask_H]\n        \n        if mask_G == 0 and mask_H == 0:\n            dp[mask_G][mask_H] = 0\n            return 0\n        \n        res = float('inf')\n        for i in range(N):\n            if mask_G & (1 << i):\n                for j in range(i + 1, N):\n                    if mask_G & (1 << j):\n                        new_mask_G = mask_G ^ (1 << i) ^ (1 << j)\n                        new_mask_H = mask_H\n                        if (i, j) not in adj_G or (j, i) not in adj_G:\n                            new_mask_H ^= (1 << i) ^ (1 << j)\n                        res = min(res, A[i][j] + dfs(new_mask_G, new_mask_H))\n        \n        dp[mask_G][mask_H] = res\n        return res\n    \n    print(dfs((1 << N) - 1, (1 << N) - 1))\n\nmin_cost_to_isomorphic()", "```python\nimport sys\nfrom itertools import permutations\n\ndef read_input():\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    N = int(data[index])\n    index += 1\n    \n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n    \n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n    \n    A = []\n    for i in range(N):\n        row = []\n        for j in range(i + 1, N):\n            row.append(int(data[index]))\n            index += 1\n        A.append(row)\n    \n    return N, M_G, edges_G, M_H, edges_H, A\n\ndef is_isomorphic(G, H):\n    return sorted(G[i] for i in range(len(G))) == sorted(H[i] for i in range(len(H)))\n\ndef toggle_edge(A, i, j):\n    if A[i][j] == 0:\n        A[i][j] = 1\n        A[j][i] = 1\n    else:\n        A[i][j] = 0\n        A[j][i] = 0\n\ndef min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A):\n    min_cost = float('inf')\n    \n    # Convert edge lists to adjacency matrices\n    G = [[0] * N for _ in range(N)]\n    for u, v in edges_G:\n        G[u][v] = 1\n        G[v][u] = 1\n    \n    H = [[0] * N for _ in range(N)]\n    for a, b in edges_H:\n        H[a][b] = 1\n        H[b][a] = 1\n    \n    # Try all permutations of H\n    for perm in permutations(range(N)):\n        current_cost = 0\n        H_copy = [row[:] for row in H]\n        \n        for i in range(N):\n            for j in range(i + 1, N):\n                if G[perm[i]][perm[j]] != H_copy[i][j]:\n                    current_cost += A[i][j]\n                    toggle_edge(H_copy, i, j)\n        \n        if is_isomorphic(G, H_copy):\n            min_cost = min(min_cost, current_cost)\n    \n    return min_cost\n\ndef main():\n    N, M_G, edges_G, M_H, edges_H, A = read_input()\n    result = min_cost_to_isomorphic(N, M_G, edges_G, M_H, edges_H, A)\n    print(result)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n\n    M_G = int(data[index])\n    index += 1\n    edges_G = []\n    for _ in range(M_G):\n        u = int(data[index]) - 1\n        v = int(data[index + 1]) - 1\n        edges_G.append((u, v))\n        index += 2\n\n    M_H = int(data[index])\n    index += 1\n    edges_H = []\n    for _ in range(M_H):\n        a = int(data[index]) - 1\n        b = int(data[index + 1]) - 1\n        edges_H.append((a, b))\n        index += 2\n\n    A = [[int(data[index + i * N + j]) for j in range(N)] for i in range(N)]\n    index += N * N\n\n    # Create adjacency lists for both graphs\n    adj_G = [[] for _ in range(N)]\n  