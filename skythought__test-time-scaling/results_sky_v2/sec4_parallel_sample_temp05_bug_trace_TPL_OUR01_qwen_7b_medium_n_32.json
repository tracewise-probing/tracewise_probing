ython\ndef count_abc_substrings(s):\n    n = len(s)\n    count = 0\n    for i in range(n - 2):\n        if s[i:i+3] == \"ABC\":\n            count += 1\n    return count\n\ndef process_queries(n, q, s, queries):\n    result = []\n    abc_count = count_abc_substrings(s)\n    result.append(abc_count)\n    \n    for x, c in queries:\n        x -= 1  # Convert to 0-based index\n        if s[x] == c:\n            result.append(abc_count)\n            continue\n        \n        # Update the string\n        s = s[:x] + c + s[x+1:]\n        \n        # Adjust the count of \"ABC\" substrings\n        if x > 0:\n            if s[x-1:x+3] == \"ABC\":\n                abc_count += 1\n            if s[x-2:x+2] == \"ABC\":\n                abc_count -= 1\n        if x < n - 2:\n            if s[x:x+3] == \"ABC\":\n                abc_count += 1\n            if s[x-1:x+2] == \"ABC\":\n                abc_count -= 1\n        \n        result.append(abc_count)\n    \n    return result\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    n = int(data[0])\n    q = int(data[1])\n    s = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(q)]\n    \n    result = process_queries(n, q, s, queries)\n    for r in result:\n        print(r)\n\nif __name__ == \"__main__\":\n    main()\n```", "def main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n\n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(Q)]\n\n    def count_ABC(s):\n        return s.count('ABC')\n\n    def update_and_count(S, pos, new_char):\n        old_char = S[pos - 1]\n        if old_char == 'A':\n            if pos >= 3 and S[pos - 3] == 'A' and S[pos - 2] == 'B' and S[pos - 1] == 'C':\n                return -1\n        elif old_char == 'B':\n            if pos >= 2 and S[pos - 2] == 'A' and S[pos - 1] == 'B':\n                return -1\n        elif old_char == 'C':\n            if pos >= 1 and S[pos - 1] == 'C':\n                return -1\n\n        S = S[:pos - 1] + new_char + S[pos:]\n        return count_ABC(S)\n\n    result = []\n    current_count = count_ABC(S)\n    result.append(current_count)\n\n    for pos, new_char in queries:\n        new_count = update_and_count(S, pos, new_char)\n        if new_count != -1:\n            current_count = new_count\n            result.append(current_count)\n        else:\n            result.append(current_count)\n\n    for r in result:\n        print(r)\n\nmain()", "def process_queries(N, Q, S, queries):\n    # Initialize prefix and suffix arrays\n    prefix_a = [0] * (N + 1)\n    prefix_b = [0] * (N + 1)\n    prefix_c = [0] * (N + 1)\n    suffix_a = [0] * (N + 1)\n    suffix_b = [0] * (N + 1)\n    suffix_c = [0] * (N + 1)\n\n    # Fill prefix arrays\n    for i in range(1, N + 1):\n        prefix_a[i] = prefix_a[i - 1]\n        prefix_b[i] = prefix_b[i - 1]\n        prefix_c[i] = prefix_c[i - 1]\n        if S[i - 1] == 'A':\n            prefix_a[i] += 1\n        elif S[i - 1] == 'B':\n            prefix_b[i] += 1\n        elif S[i - 1] == 'C':\n            prefix_c[i] += 1\n\n    # Fill suffix arrays\n    for i in range(N - 1, -1, -1):\n        suffix_a[i] = suffix_a[i + 1]\n        suffix_b[i] = suffix_b[i + 1]\n        suffix_c[i] = suffix_c[i + 1]\n        if S[i] == 'A':\n            suffix_a[i] += 1\n        elif S[i] == 'B':\n            suffix_b[i] += 1\n        elif S[i] == 'C':\n            suffix_c[i] += 1\n\n    # Process each query\n    results = []\n    for X, C in queries:\n        X -= 1  # Convert to 0-indexed\n        old_char = S[X]\n        new_char = C\n\n        # Update prefix and suffix arrays\n        if old_char == 'A':\n            prefix_a[X] -= 1\n        elif old_char == 'B':\n            prefix_b[X] -= 1\n        elif old_char == 'C':\n            prefix_c[X] -= 1\n\n        if new_char == 'A':\n            prefix_a[X] += 1\n        elif new_char == 'B':\n            prefix_b[X] += 1\n        elif new_char == 'C':\n            prefix_c[X] += 1\n\n        # Calculate the number of 'ABC' substrings\n        count = 0\n        for i in range(X, N - 1):\n            if prefix_a[i] > 0 and prefix_b[i] > 0 and suffix_c[i + 1] > 0:\n                count += 1\n\n        results.append(count)\n\n    return results\n\n# Read input from standard input\nimport sys\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nQ = int(data[1])\nS = data[2]\nqueries = [(int(data[2 + 2 * i]), data[3 + 2 * i]) for i in range(Q)]\n\n# Process queries and print results\nresults = process_queries(N, Q, S, queries)\nfor result in results:\n    print(result)", "```python\ndef count_abc_substrings(S):\n    n = len(S)\n    count = 0\n    for i in range(n - 2):\n        if S[i:i+3] == \"ABC\":\n            count += 1\n    return count\n\ndef process_queries(N, Q, S, queries):\n    result = []\n    current_count = count_abc_substrings(S)\n    result.append(current_count)\n    \n    for X, C in queries:\n        X -= 1  # Convert to 0-based index\n        if S[X] != C:\n            current_count -= S[X-1:X+2] == \"ABC\"\n            current_count += S[X-1:X+2] == \"ABC\"\n            S = S[:X] + C + S[X+1:]\n            result.append(current_count)\n    \n    return result\n\ndef main():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 + 2 * i]) - 1, data[2 + 2 * i + 1]) for i in range(Q)]\n    \n    result = process_queries(N, Q, S, queries)\n    for r in result:\n        print(r)\n\nif __name__ == \"__main__\":\n    main()\n```", "```python\ndef process_queries():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    N = int(data[0])\n    Q = int(data[1])\n    S = data[2]\n    queries = [(int(data[2 * i + 3]), data[2 * i + 4]) for i in range(Q)]\n    \n    # Function to count occurrences of \"ABC\" in the string\n    def count_ABC(s):\n        count = 0\n        for i in range(2, len(s)):\n            if s[i] == 'C' and s[i - 1] == 'B' and s[i - 2] == 'A':\n                count += 1\n        return count\n    \n    # Initial count of \"ABC\"\n    initial_count = count_ABC(S)\n    print(initial_count)\n    \n    # Process each query\n    current_count = initial_count\n    for X, C in queries:\n        if S[X - 1] == C:\n            continue\n        # Update the string\n        S = S[:X - 1] + C + S[X:]\n        # Adjust the count of \"ABC\"\n        if X >= 3:\n            if S[X - 3] == 'A' and S[X - 2] == 'B' and S[X - 1] == 'C':\n                current_count += 1\n            if S[X - 4] == 'A' and S[X - 3] == 'B' and S[X - 2] == 'C':\n                current_count -= 1\n        print(current_count)\n\nprocess_queries()", "def process_queries(N, Q, S, queries):\n    # Initialize counts for the sliding window\n    count_ABC = 0\n    count_A = [0] * (N + 1)\n    count_AB = [0] * (N + 1)\n    count_B = [0] * (N + 1)\n\n    # Helper function to update counts\n    def update_counts(index, old_char, new_char):\n        nonlocal count_ABC, count_A, count_AB, count_B\n        if old_char == 'A':\n            count_A[index] -= 1\n            if index > 0:\n                count_AB[index] -= count_B[index - 1]\n                count_ABC -= count_AB[index]\n        elif old_char == 'B':\n            count_AB[index] -= count_B[index - 1]\n            count_ABC -= count_AB[index]\n        elif old_char == 'C':\n            count_ABC -= count_AB[index]\n\n        if new_char == 'A':\n            count_A[index] += 1\n            if index > 0:\n                count_AB[index] += count_B[index - 1]\n                count_ABC += count_AB[index]\n        elif new_char == 'B':\n            count_AB[index] += count_B[index - 1]\n            count_ABC += count_AB[index]\n        elif new_char == 'C':\n            count_ABC += count_AB[index]\n\