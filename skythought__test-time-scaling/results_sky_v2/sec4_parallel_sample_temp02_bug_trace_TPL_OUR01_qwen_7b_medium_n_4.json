{"task_id": "abc371_c", "codes": ["import sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorph(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n\n    # Generate all permutations of vertices in H\n    for perm in permutations(range(1, N+1)):\n        H_perm = [[perm[a], perm[b]] for a, b in H]\n\n        # Check if H_perm is isomorphic to G\n        if set(tuple(sorted(edge)) for edge in H_perm) == set(tuple(sorted(edge)) for edge in G):\n            current_cost = sum(A[perm[i]-1][perm[j]-1] for i in range(N) for j in range(i+1, N) if (perm[i], perm[j]) not in H_perm and (perm[j], perm[i]) not in H_perm)\n            min_cost = min(min_cost, current_cost)\n\n    return min_cost if min_cost != float('inf') else 0\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G = [tuple(map(int, data[index:index+2])) for _ in range(M_G)]\n    index += 2 * M_G\n    M_H = int(data[index])\n    index += 1\n    H = [tuple(map(int, data[index:index+2])) for _ in range(M_H)]\n    index += 2 * M_H\n    A = [[int(data[index + i*N + j]) for j in range(N)] for i in range(N)]\n\n    result = min_cost_to_isomorph(G, H, A)\n    print(result)", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs):\n    # Create adjacency lists for both graphs\n    adj_g = defaultdict(list)\n    adj_h = defaultdict(list)\n\n    for u, v in edges_g:\n        adj_g[u].append(v)\n        adj_g[v].append(u)\n\n    for a, b in edges_h:\n        adj_h[a].append(b)\n        adj_h[b].append(a)\n\n    # Initialize DP table\n    dp = [[float('inf')] * (1 << n) for _ in range(1 << n)]\n    dp[0][0] = 0\n\n    # Iterate over all possible subsets of vertices for G and H\n    for mask_g in range(1 << n):\n        for mask_h in range(1 << n):\n            if dp[mask_g][mask_h] == float('inf'):\n                continue\n\n            # Check all pairs of vertices\n            for i in range(1, n + 1):\n                for j in range(i + 1, n + 1):\n                    bit_i = 1 << (i - 1)\n                    bit_j = 1 << (j - 1)\n\n                    # Calculate the current state based on the previous state\n                    new_mask_g = mask_g ^ bit_i ^ bit_j\n                    new_mask_h = mask_h ^ bit_i ^ bit_j\n\n                    # Update the DP table\n                    if (bit_i & mask_g) != 0 and (bit_j & mask_g) != 0:\n                        if (bit_i & mask_h) == 0 or (bit_j & mask_h) == 0:\n                            dp[new_mask_g][new_mask_h] = min(dp[new_mask_g][new_mask_h], dp[mask_g][mask_h] + costs[i - 1][j - 1])\n                    elif (bit_i & mask_g) == 0 or (bit_j & mask_g) == 0:\n                        if (bit_i & mask_h) != 0 and (bit_j & mask_h) != 0:\n                            dp[new_mask_g][new_mask_h] = min(dp[new_mask_g][new_mask_h], dp[mask_g][mask_h] + costs[i - 1][j - 1])\n\n    return dp[(1 << n) - 1][(1 << n) - 1]\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    n = int(data[index])\n    index += 1\n    m_g = int(data[index])\n    index += 1\n    edges_g = []\n    for _ in range(m_g):\n        u = int(data[index])\n        index += 1\n        v = int(data[index])\n        index += 1\n        edges_g.append((u, v))\n\n    m_h = int(data[index])\n    index += 1\n    edges_h = []\n    for _ in range(m_h):\n        a = int(data[index])\n        index += 1\n        b = int(data[index])\n        index += 1\n        edges_h.append((a, b))\n\n    costs = []\n    for _ in range(n - 1):\n        row = list(map(int, data[index:index + n - 1]))\n        index += n - 1\n        costs.append(row)\n\n    result = min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs)\n    print(result)", "d", "i"], "passed": [false, false, false, false], "raw_code": ["import sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorph(G, H, A):\n    N = len(G)\n    min_cost = float('inf')\n\n    # Generate all permutations of vertices in H\n    for perm in permutations(range(1, N+1)):\n        H_perm = [[perm[a], perm[b]] for a, b in H]\n\n        # Check if H_perm is isomorphic to G\n        if set(tuple(sorted(edge)) for edge in H_perm) == set(tuple(sorted(edge)) for edge in G):\n            current_cost = sum(A[perm[i]-1][perm[j]-1] for i in range(N) for j in range(i+1, N) if (perm[i], perm[j]) not in H_perm and (perm[j], perm[i]) not in H_perm)\n            min_cost = min(min_cost, current_cost)\n\n    return min_cost if min_cost != float('inf') else 0\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    N = int(data[index])\n    index += 1\n    M_G = int(data[index])\n    index += 1\n    G = [tuple(map(int, data[index:index+2])) for _ in range(M_G)]\n    index += 2 * M_G\n    M_H = int(data[index])\n    index += 1\n    H = [tuple(map(int, data[index:index+2])) for _ in range(M_H)]\n    index += 2 * M_H\n    A = [[int(data[index + i*N + j]) for j in range(N)] for i in range(N)]\n\n    result = min_cost_to_isomorph(G, H, A)\n    print(result)", "import sys\nfrom collections import defaultdict\n\ndef min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs):\n    # Create adjacency lists for both graphs\n    adj_g = defaultdict(list)\n    adj_h = defaultdict(list)\n\n    for u, v in edges_g:\n        adj_g[u].append(v)\n        adj_g[v].append(u)\n\n    for a, b in edges_h:\n        adj_h[a].append(b)\n        adj_h[b].append(a)\n\n    # Initialize DP table\n    dp = [[float('inf')] * (1 << n) for _ in range(1 << n)]\n    dp[0][0] = 0\n\n    # Iterate over all possible subsets of vertices for G and H\n    for mask_g in range(1 << n):\n        for mask_h in range(1 << n):\n            if dp[mask_g][mask_h] == float('inf'):\n                continue\n\n            # Check all pairs of vertices\n            for i in range(1, n + 1):\n                for j in range(i + 1, n + 1):\n                    bit_i = 1 << (i - 1)\n                    bit_j = 1 << (j - 1)\n\n                    # Calculate the current state based on the previous state\n                    new_mask_g = mask_g ^ bit_i ^ bit_j\n                    new_mask_h = mask_h ^ bit_i ^ bit_j\n\n                    # Update the DP table\n                    if (bit_i & mask_g) != 0 and (bit_j & mask_g) != 0:\n                        if (bit_i & mask_h) == 0 or (bit_j & mask_h) == 0:\n                            dp[new_mask_g][new_mask_h] = min(dp[new_mask_g][new_mask_h], dp[mask_g][mask_h] + costs[i - 1][j - 1])\n                    elif (bit_i & mask_g) == 0 or (bit_j & mask_g) == 0:\n                        if (bit_i & mask_h) != 0 and (bit_j & mask_h) != 0:\n                            dp[new_mask_g][new_mask_h] = min(dp[new_mask_g][new_mask_h], dp[mask_g][mask_h] + costs[i - 1][j - 1])\n\n    return dp[(1 << n) - 1][(1 << n) - 1]\n\nif __name__ == '__main__':\n    input = sys.stdin.read\n    data = input().split()\n\n    index = 0\n    n = int(data[index])\n    index += 1\n    m_g = int(data[index])\n    index += 1\n    edges_g = []\n    for _ in range(m_g):\n        u = int(data[index])\n        index += 1\n        v = int(data[index])\n        index += 1\n        edges_g.append((u, v))\n\n    m_h = int(data[index])\n    index += 1\n    edges_h = []\n    for _ in range(m_h):\n        a = int(data[index])\n        index += 1\n        b = int(data[index])\n        index += 1\n        edges_h.append((a, b))\n\n    costs = []\n    for _ in range(n - 1):\n        row = list(map(int, data[index:index + n - 1]))\n        index += n - 1\n        costs.append(row)\n\n    result = min_cost_to_isomorphic(n, m_g, edges_g, m_h, edges_h, costs)\n    print(result)", "```python\ndef min_cost_to_isomorphic():\n    import sys\n    input = sys.stdin.read\n    data = input().split()\n    \n    index = 0\n    def next_int():\n        nonlocal index\n        result = int(data[index])\n        index += 1\n        return result\n    \n    N = next_int()\n    M_G = next_int()\n    G_edges = []\n    for _ in range(M_G):\n        u = next_int() - 1\n        v = next_int() - 1\n        G_edges.append((u, v))\n    \n    M_H = next_int()\n    H_edges = []\n    for _ in range(M_H):\n        a = next_int() - 1\n        b = next_int() - 1\n        H_edges.append((a, b))\n    \n    A = [[0] * N for _ in range(N)]\n    for i in range(1, N):\n        for j in range(i + 1, N):\n            A[i][j] = next_int()\n    \n    # Convert edge lists to sets for easier lookup\n    G_set = set(G_edges)\n    H_set = set(H_edges)\n    \n    # Initialize DP table\n    dp = [float('inf')] * (1 << N)\n    dp[0] = 0\n    \n    # Iterate over all possible states\n    for mask in range(1 << N):\n        for i in range(N):\n            for j in range(i + 1, N):\n                bit = 1 << i | 1 << j\n                if (mask & bit) == bit:\n                    # Edge (i, j) is present in H\n                    if (i, j) not in G_set:\n                        dp[mask] = min(dp[mask], dp[mask ^ bit] + A[i][j])\n                elif (i, j) in G_set:\n                    dp[mask] = min(dp[mask], dp[mask ^ bit] + A[i][j])\n    \n    print(dp[(1 << N) - 1])\n\nmin_cost_to_isomorphic()", "```python\nimport sys\nfrom itertools import permutations\n\ndef min_cost_to_isomorph(G, H, A):\n    N = len(G)\n    \n    def get_adj_matrix(graph):\n        adj_matrix = [[0] * N for _ in range(N)]\n        for u, v in graph:\n            adj_matrix[u-1][v-1] = 1\n            adj_matrix[v-1][u-1] = 1\n        return adj_matrix\n    \n    def calculate_cost(current_H, target_G):\n        cost = 0\n        for i in range(N):\n            for j in range(i+1, N):\n                if current_H[i][j] != target_G[i][j]:\n                    cost += A[i][j]\n        return cost\n    \n    G_adj = get_adj_matrix(G)\n    min_cost = float('inf')\n    \n    for perm in permutations(range(1, N+1)):\n        H_perm = get_adj_matrix([(perm[a-1], perm[b-1]) for a, b in H])\n        cost = calculate_cost(H_perm, G_adj)\n        min_cost = min(min_cost, cost)\n    \n    return min_cost\n\n# Read input\ninput = sys.stdin.read\ndata = input().split()\n\nN = int(data[0])\nM_G"]}
